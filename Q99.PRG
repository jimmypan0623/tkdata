CLEAR 
SET EXCLUSIVE OFF 
SET DATE TO TAIWAN 
YEAR_MONTH_STR=''
TALA1=0 &&原定交期總筆數
TALA2=0 &&原定交期每日筆數
TALB1=0 &&逾期總筆數
TALB2=0 &&逾期每日筆數
DECLARE Q99A(31) &&原定交期筆數
DECLARE Q99B(31) &&逾期筆數
*************************建立表單Q99AFORM*************************          
Q99AFORM=CREATEOBJECT("TKQ99A")
Q99AFORM.SHOW 
DEFINE CLASS TKQ99A AS FORM
*!*	  AUTOCENTER=.T.
  TOP=INT_015* 200
  LEFT=INT_015* 250
  CAPTION='Q99. JW逾期交貨一覽表'
  FONTSIZE=INT_015*9
  HEIGHT=INT_015*200
