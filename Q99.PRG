      NAME='CMND1'
 ADD OBJECT CMND2 AS COMMANDBUTTON WITH;
      LEFT=INT_015*160,;
      TOP=INT_015*140,;
      HEIGHT=INT_015*40,;
      WIDTH=INT_015*80,;
      FONTSIZE=12,;
      CAPTION='\<X.離開',;
      TOOLTIPTEXT='離開此查詢尋畫面!快速鍵->ALT+X'
      NAME='CMND2'
***********************      
 PROCEDURE INIT 
         THISFORM.SETALL('FONTSIZE',INT_015*12,'LABEL')
         THISFORM.SETALL('FONTSIZE',INT_015*12,'TEXTBOX')
         THISFORM.TXTF01.READONLY=.F.
         THISFORM.TXTF01.VALUE=LEFT(DTOC(DATE()),5)
      ENDPROC 
 ********確認鍵******
PROCEDURE CMND1.CLICK     
YEAR_MONTH_STR=LEFT(DTOS(CTOD(THISFORM.TXTF01.VALUE+'/01')),6)
     DO CASE
            CASE  EMPTY(THISFORM.TXTF01.VALUE)=.T.
                        =MESSAGEBOX('查詢日期不得為空白',0+64,'提示視窗')
                        THISFORM.TXTF01.VALUE=''
            		THISFORM.TXTF01.SETFOCUS
                        RETURN
           CASE   THISFORM.TXTF01.VALUE>DTOC(DATE())
             		=MESSAGEBOX('查詢年份日期尚未到該故無資料,請重新輸入!',0+64,'提示視窗')
     			THISFORM.TXTF01.VALUE=''
            		THISFORM.TXTF01.SETFOCUS
                        RETURN 
     ENDCASE 
  IF  FILE('P19'+ALLTRIM(YEAR_MONTH_STR)+'.DBF')=.F.
             		=MESSAGEBOX(LEFT(ALLTRIM(YEAR_MONTH_STR),4)+'年'+RIGHT(ALLTRIM(YEAR_MONTH_STR),2)+'月份並無資料,請重新輸入!',0+64,'提示視窗')
     			THISFORM.TXTF01.VALUE=''
            		THISFORM.TXTF01.SETFOCUS
                        RETURN	 
 ELSE
	F='P19'+ALLTRIM(YEAR_MONTH_STR)+'.DBF' 
	S=ALLTRIM(STR(VAL(LEFT(ALLTRIM(YEAR_MONTH_STR),4))-1911)+'/'+RIGHT(ALLTRIM(YEAR_MONTH_STR),2))
***************判斷原定交期筆數
	SELECT F01,F02,F05,F22 FROM &F WHERE !EMPTY(F22) AND  LEFT(ALLTRIM(DTOC(F22)),5)=S INTO CURSOR Q991
	TALA1=RECCOUNT('Q991')
	FOR I=1 TO 31
		IF  I<10
			H='0'+ALLTRIM(STR(I))
		ELSE
			H=ALLTRIM(STR(I))
		ENDIF
		SELECT * FROM Q991 WHERE  RIGHT(ALLTRIM(DTOC(F01)),2)=H INTO CURSOR Q993
		SELECT Q993
		TALA2=RECCOUNT('Q993')
		STORE TALA2 TO Q99A(I)
	ENDFOR
*****************判斷逾期筆數
	SELECT * FROM Q991 WHERE F01>F22   INTO CURSOR Q992
	TALB1=RECCOUNT('Q992')
	FOR J=1 TO 31
		IF  J<10
			H='0'+ALLTRIM(STR(J))
		ELSE
			H=ALLTRIM(STR(J))
		ENDIF
		SELECT * FROM Q992 WHERE  RIGHT(ALLTRIM(DTOC(F01)),2)=H INTO CURSOR Q993
		SELECT Q993
		TALB2=RECCOUNT('Q993')
		STORE TALB2 TO Q99B(J)
	ENDFOR
ENDIF	
      TKQ99B=CREATEOBJECT("TKQ99B")  
      TKQ99B.SHOW	
     THISFORM.CMND2.CLICK	
ENDPROC     			 
********************
 PROCEDURE CMND2.CLICK       
          CLOSE TABLE ALL
          THISFORM.RELEASE 
           RETURN  
 ENDPROC      
