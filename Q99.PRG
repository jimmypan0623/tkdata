  CONTROLBOX=.F.
  BORDERSTYLE=2
  SHOWTIPS=.T.
  SHOWWINDOW=1
  WINDOWTYPE=1
  NAME='TKQ99A'
  ADD OBJECT LBLF01 AS LABEL WITH;
      LEFT=INT_015*5,;
      TOP=INT_015*40,;
      WIDTH=INT_015*190,;
      HEIGHT=INT_015*30,;
      CAPTION='請輸入預查詢之年月份'
  ADD OBJECT TXTF01 AS TEXTBOX WITH;
      LEFT=INT_015*168,;
      TOP=INT_015*35,;
      HEIGHT=INT_015*30,;
      WIDTH=INT_015*50,;
      MAXLENGTH=5,;
