GO TOP
DO WHILE !EOF()
   SELECT S21
   FOR I=1 TO 2
       PAN=IIF(I=1,KAU.upgettime,KAU.downgettim)
       IF !EMPTY(ALLTRIM(PAN))
          APPEND BLANK
          REPLACE F01 WITH KAU.holderno
          REPLACE F02 WITH IIF(SEEK(F01,'S50'),S50.F02,'')
          REPLACE F03 WITH KAU.CARDNO
          REPLACE F04 WITH KAU.kqdate
          REPLACE F05 WITH ALLTRIM(STR(I))
          REPLACE F06 WITH PAN
       ENDIF   
   ENDFOR
   SELECT KAU
   SKIP
