CLOSE ALL
CLEAR ALL
SET ENGINEBEHAVIOR 70
*!*	SET DEFAULT TO Z:\MIS\SALES
SET EXCLUSIVE OFF
IF !USED('B01')
   SELE 0
   USE B01 
ELSE
   SELE B01
ENDIF
SET ORDER TO 1
IF !USED('LKK')
   SELE 0
   USE LKK 
ELSE
   SELE LKK
ENDIF
SELECT * FROM LKK GROUP BY F01 ORDER BY F01 INTO CURSOR LKK_TEMP
SELECT LKK_TEMP
GO TOP
DO WHILE !EOF()
      SELECT B01
      SEEK LKK_TEMP.F01
      IF FOUND()
           REPLACE B01.F99 WITH ALLTRIM(LKK_TEMP.F02)
      ENDIF
      SELECT LKK_TEMP
      SKIP
ENDDO
SELECT B01
BROWSE
