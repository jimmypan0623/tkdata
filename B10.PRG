       ELSE
          SELE B10
       ENDIF
       SET ORDER TO 1      
       SET RELA TO F03 INTO B01 ADDI
       SET RELATION TO F06 INTO C01 ADDI
       CREATE CURSOR B10_1;
       (F01 C(10),F02 C(2),F05 C(5),F07 C(20),F09 C(1),F10 C(17),F90 C(90))
       INDE ON F01 TAG B10_11
       INDE ON F05 TAG B10_12
       SELE F01,F02,F05,F07,F09,F10,F90 FROM B10 GROUP BY F01 ORDER BY F01 INTO CURSOR B10_2
       SELE B10_2
       GO TOP
       DO WHILE !EOF()
          SELE B10_1
          APPEND BLANK
          REPL F01 WITH B10_2.F01
          REPL F02 WITH B10_2.F02
          REPL F05 WITH B10_2.F05
          REPL F07 WITH B10_2.F07
          REPL F09 WITH B10_2.F09
          REPL F10 WITH B10_2.F10
          REPL F90 WITH B10_2.F90
          SELE B10_2
          SKIP
       ENDDO
       SELE B10_2
       USE   
       SELE B10_1
       SET ORDER TO 1
       SET RELA TO F05 INTO A14       
       THISFORM.GRID1.RECORDSOURCE='B10_1'
       THISFORM.GRID1.COLUMN1.CONTROLSOURCE='B10_1.F01'
       THISFORM.GRID1.COLUMN2.CONTROLSOURCE='B10_1.F05'
