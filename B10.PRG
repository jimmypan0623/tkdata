       IF !USED('&B39')
          SELE 0
          USE (B39) ALIA B39
       ELSE
          SELE B39
       ENDIF
       SET ORDER TO 1                                  
       SELE B10
       SET RELATION OFF INTO C01
       set rela off into b01
       SET RELA OFF INTO B10_1
       USE
       SELE B10_1
       SET RELA OFF INTO A14
       USE
       B10='B10'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       B101='B101'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       IF !USED('&B10')
