CLOSE ALL
CLEAR ALL
SET DATE TO YMD
SELECT 0
USE JWPRICE1
GO TOP 
BK=F14
DT=F99
DO WHILE !EOF()
   IF EMPTY(F14)
      REPLACE F14 WITH BK
      REPLACE F99 WITH DT
   ELSE
       BK=F14
       DT=F99  
   ENDIF
   IF AT('(',F03)=1
      REPLACE F03 WITH STUFF(F03,0,3,'')
   ENDIF
   IF AT('向',F03)>0
      REPLACE F03 WITH STUFF(F03,AT('向',F03),LEN(F03)-AT('向',F03),'')
      
   ENDIF
   IF AT('貴',F03)>0
       REPLACE F03 WITH STUFF(F03,AT('貴',F03),LEN(F03)-AT('貴',F03),'')
   ENDIF
   IF AT('賣',F03)>0
       REPLACE F03 WITH STUFF(F03,AT('賣',F03),LEN(F03)-AT('賣',F03),'')
   ENDIF
   SELECT JWPRICE1
   SKIP
  
ENDDO
DO WHILE !BOF()
   REPLACE F02 WITH CTOD(LEFT(ALLTRIM(F99),4)+'/'+SUBSTR(ALLTRIM(F99),6,2)+'/'+RIGHT(ALLTRIM(F99),2))
      
   IF AT('德',F03)>0
       REPLACE F03 WITH STUFF(F03,AT('德',F03),LEN(F03)-AT('德',F03),'')
   ENDIF
   SKIP -1
ENDDO   