CLEAR ALL
CLOSE ALL
SET DELETED ON
SELECT 0
USE A23
SET ORDER TO 1
GO TOP 
DO WHILE !EOF()
   YMNAME=LEFT(DTOS(F01),6)
   TBNAME='C10'+YMNAME
   IDXNAME='C109'+YMNAME
   IF FILE(TBNAME+'.DBF')
      ?TBNAME
      SELECT 0
      USE &TBNAME EXCLUSIVE
      SET ORDER TO 1
      INDEX ON F79+F01 TAG &IDXNAME
      USE
   ENDIF
   SELECT A23
   SKIP
ENDDO
CLOSE ALL
CLEAR ALL