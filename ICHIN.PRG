SET DELETED ON
SET EXCLUSIVE OFF
CLOSE ALL

SELECT 0
USE ICHNM
SET ORDER TO 1
SELECT 0
USE JAUILIN
GO TOP
DO WHILE !EOF()
   REPLACE F01 WITH IIF(SEEK(LEFT(JAUILIN.F02,AT('¤§',JAUILIN.F02)-1),'ICHNM'),ICHNM.F01,'')
   
   
   SELECT JAUILIN
   SKIP
ENDDO
SELECT JAUILIN
DO WHILE !BOF()
   ACHR=SUBSTR(JAUILIN.f02,AT('¤§',JAUILIN.f02)+2,ABS(AT(' ',JAUILIN.f02)-AT('¤§',JAUILIN.f02)-2))
   REPLACE F01 WITH ALLTRIM(F01)+IIF(SEEK(ACHR,'ICHNM'),ICHNM.F01,'')
   SELECT JAUILIN
   SKIP -1
ENDDO   