*******************************************   計算進貨金額
FUNC TTL1
     PARA BKK_NO
     qt_od=0
     sele E20
     SEEK BKK_NO
     IF FOUND()
        DO WHILE F03=BKK_NO            
           IF LEFT(E20.F02,2)='BM' AND  LEFT(E20.F04,4)<>'稅額'
                       IF LEFT(E20.F19,2)<>'02'                   
                          qt_od=qt_od+f08
                      ELSE     
                          qt_od=qt_od+ROUND(f08/(1+INT_002/100),INT_070)               &&二聯式應稅
                      ENDIF                                   
           ENDIF   
           SKIP
        ENDDO
     ELSE
       qt_od=0
     ENDIF
     SELE E20_1
     RETURN ROUND(qt_od,INT_070)
