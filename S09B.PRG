 PROC INVERSE_BOTT.CLICK     &&反確認作業  
      IF  MESSAGEBOX('您是否要反確認此筆資料!',4+32+256,'請確認')=6
          SELECT S09
          LOCK()
          IF RLOCK()
              IF  S09.F15<>'Y'     &&判斷是否已確認
                   =MESSAGEBOX('此筆資料尚未確認過,請查明後再作反確認!',0+64,'提示訊息視窗')
                   THISFORM.GRID2.SETFOCUS    
              ELSE                      
                  SELECT S09
                  REPLACE S09.F15 WITH 'N'  	      
    		  REPLACE S09.F99 WITH DTOC(DATE())+' '+IIF(SEEK(SYS_OPER,'A01'),A01.F03,'')          
                  ***判斷是否為合併薪資發放  1.單獨 2.合併  若為合併則判斷S10.薪資計算作業中是否存在且尚未確認,若符合則刪除
                   IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP07.VALUE<>1  
                        S1=LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
          	        FILE_S10='S10'+ALLTRIM(S1)
		        FIND_S10=ALLTRIM(FILE_S10)+'.DBF'
		        S102='S102'+LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
		        S103='S103'+LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
		        IF  FILE('&FIND_S10')=.T.   
		              IF  !USED('&FIND_S10')
		                   SELE 0
			           USE &FIND_S10 
		              ELSE
			           SELE &FILE_S10
		              ENDIF  
		              SELECT &FILE_S10
		              SET ORDER TO S102   && F01+F97+F18+F19+DTOS(F22)
		              ****找出未確認且已新增C.薪資所得明細
                              SELECT * FROM &FILE_S10 WHERE ALLTRIM(&FILE_S10..F03)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE);
 	                                     AND ALLTRIM(&FILE_S10..F19)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE) AND &FILE_S10..F22=THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE;
 	                                     AND  &FILE_S10..F18='I'  AND &FILE_S10..F97='C'  AND &FILE_S10..F98='N' INTO CURSOR S10C
 	                      SELECT S10C
 	                      GO TOP
 	                      DO WHILE !EOF()	              
 	                             SELECT &FILE_S10
 	                             SEEK S10C.F01+'C'+'I'+S10C.F19+DTOS(S10C.F22)
 	                             IF FOUND()
 	                                 DELETE 
 	                             ENDIF
 	                             SELECT S10C
 	                             SKIP
 	                      ENDDO
 	                      SELECT S10C
 	                      USE
		              *****找出D.薪資扣款明細中之其他所得扣稅額
		              SELECT &FILE_S10
		              SET ORDER TO  S103 && F01+F97+F24+F25
                              SELECT  * FROM &FILE_S10 WHERE ALLTRIM(&FILE_S10..F03)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE);
 	                                     AND ALLTRIM(&FILE_S10..F25)=ALLTRIM(ALLTRIM(THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE)+'扣稅額');
 	                                     AND  &FILE_S10..F24='N'  AND &FILE_S10..F97='D'  AND &FILE_S10..F98='N' INTO CURSOR S10D
 	                      SELECT S10D
 	                      GO TOP
 	                      DO WHILE !EOF()	              
 	                             SELECT &FILE_S10
 	                             SEEK S10D.F01+'D'+'N'+S10D.F25
 	                             IF FOUND()
 	                                 DELETE 
 	                             ENDIF
 	                             SELECT S10D
 	                             SKIP
 	                      ENDDO
			      SELECT &FILE_S10
			      USE 	                      
 	                      SELECT S10D
 	                      USE		              	              		              	                                     
			ENDIF
		   ENDIF
		   *****刪除S09A.員工其他所得發放作業中從部門新增之資料
                   SELECT  RECNO() FROM S09 WHERE ALLTRIM(S09.F03)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE);
 	                          AND S09.F05=THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE;
 	                          AND ALLTRIM(S09.F06)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE);
 	                          AND S09.F07=ALLTRIM(STR(THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP07.VALUE)) AND  S09.F15='Y'  AND S09.F16='3' INTO ARRAY BK3
		   JJ3=''                
	           IF _TALLY>0   
	               FOR I= 1 TO ALEN(BK3)
	                       JJ3=JJ3+ALLTRIM(STR(BK3(I)))+','
	               ENDFOR    
	               KK3=STUFF(JJ3,RAT(',',JJ3,1),1,'')              
	               RLOCK(KK3,'S09')
	               LL3=RLOCK(KK3,'S09')
	           ELSE
	               LL3=.T.   
	           ENDIF   
	           IF LL3=.T. AND LEN(ALLTRIM(JJ3))>0  
	               SELECT S09
	               FOR I= 1 TO ALEN(BK3)        
	                       GO BK3(I)   
	                       DELETE 
	               ENDFOR 
	           ENDIF
                  =MESSAGEBOX('此發放作業反確認成功!',0+64,'提示訊息視窗')  
                  THISFORM.GRID2.SETFOCUS      
            ENDIF 
         ENDIF   
       ENDIF    
       RELEASE ALL LIKE BK* 
       UNLOCK   ALL 	
       THISFORM.PAGEFRAME1.PAGE2.ACTIVATE 	  
 ENDPROC
