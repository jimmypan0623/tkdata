 PROC ANS_BOTT.CLICK     &&確認作業     
     IF  MESSAGEBOX('您是否要確認此筆資料!',4+32+256,'請確認')=6
          SELECT S09
          LOCK()
          IF RLOCK()
              IF  S09.F15='Y'       &&判斷是否已確認
                   =MESSAGEBOX('此筆資料已確認過,請查明後再作確認!',0+64,'提示訊息視窗')
                   THISFORM.GRID2.SETFOCUS    
              ELSE                      
                   SELECT S09
     	           REPLACE S09.F15 WITH 'Y'  	      
    		   REPLACE S09.F99 WITH DTOC(DATE())+' '+IIF(SEEK(SYS_OPER,'A01'),A01.F03,'')                         
              ***判斷是否為合併薪資發放  1.單獨 2.合併  若為合併則加入S10.薪資計算作業中						
                   IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP07.VALUE<>1  
                        S1=LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
          	        FILE_S10='S10'+ALLTRIM(S1)
		        FIND_S10=ALLTRIM(FILE_S10)+'.DBF'
		        S102='S102'+LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
		        IF  FILE('&FIND_S10')=.T.   
		              IF  !USED('&FIND_S10')
		                   SELE 0
			           USE &FIND_S10 
		              ELSE
			           SELE &FILE_S10
		              ENDIF  
		              SELECT &FILE_S10
		              SET ORDER TO S102   && F01+F97+F18+F19+DTOS(F22)
		              ****找出未確認且已新增A.基本考勤資料之人員
		              SELECT &FILE_S10..F01,&FILE_S10..F02,&FILE_S10..F03,&FILE_S10..F04,S05.F15 FROM &FILE_S10,S05;
		                           WHERE ALLTRIM(&FILE_S10..F03)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE);
		                           AND &FILE_S10..F97='A' AND &FILE_S10..F98<>'Y' AND &FILE_S10..F01=S05.F01 INTO CURSOR S10_TEMP  
			      SELECT S10_TEMP
	       	              GO TOP  
	       	              DO WHILE !EOF()  
	       	                     TAX_MONEY=0	   &&扣稅額
	       	                     SELECT &FILE_S10
	       	                     SEEK S10_TEMP.F01+'C'+'I';
	       	                     +ALLTRIM(THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE);
	       	                     +SPACE(30-LEN(ALLTRIM(THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE)));
	       	                     +DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE)
				     IF  !FOUND() 		&&若薪資所得中已存在相同之發放項目及放日期則不需新增
		                          SELECT &FILE_S10
			                  APPEND BLANK			                    
			                  REPLACE &FILE_S10..F01 WITH S10_TEMP.F01
				          REPLACE &FILE_S10..F02 WITH S10_TEMP.F02
			                  REPLACE &FILE_S10..F03 WITH S10_TEMP.F03
			                  REPLACE &FILE_S10..F04 WITH S10_TEMP.F04              		               
			                  REPLACE &FILE_S10..F18 WITH 'I' 
			                  REPLACE &FILE_S10..F19 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE
		                          IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP09.VALUE=1  &&發放底數
		                               SELECT S06
		                               SET ORDER TO S061 && F01+F06+F07
					       DO CASE
					             CASE  S10_TEMP.F15='M'   &&發薪種類(M:月薪 D:日薪 T:時薪)
					                        MONTH_MONEY=IIF(INDEXSEEK(S10_TEMP.F01+'A',.T.,'S06','S061'),S06.F08,0)  				&&底薪
						                DAY_MONEY=IIF(IIF(INDEXSEEK('Z',.T.,'S00','S001'),S00.F02,0)<>0,MONTH_MONEY/S00.F02,0)      &&日薪
						                TIME_MONEY=IIF(IIF(INDEXSEEK('Z',.T.,'S00','S001'),S00.F03,0)<>0,DAY_MONEY/S00.F03,0)       	&&時薪
						     CASE  S10_TEMP.F15='D'     
						     		MONTH_MONEY=0
						                DAY_MONEY=IIF(INDEXSEEK(S10_TEMP.F01+'B',.T.,'S06','S061'),S06.F08,0)     
						                TIME_MONEY=IIF(IIF(INDEXSEEK('Z',.T.,'S00','S001'),S00.F03,0)<>0,DAY_MONEY/S00.F03,0)       			  	   
						     CASE  S10_TEMP.F15='T'      
						     		MONTH_MONEY=0
						                DAY_MONEY=0
						                TIME_MONEY=IIF(INDEXSEEK(S10_TEMP.F01+'C',.T.,'S06','S061'),S06.F08,0)
					       ENDCASE		                                                    
		                               IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP12.VALUE=1   &&不扣稅
		                                    DO CASE
		                                          CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='H' 	&&時薪
		                                          	     REPLACE &FILE_S10..F20 WITH ROUND(TIME_MONEY*(THISFORM.PAGEFRAME1.PAGE2.TXTF10.VALUE/100),0) 	&&本月薪資所得
       					                  CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='D'      &&日薪
                                                                     REPLACE &FILE_S10..F20 WITH ROUND(DAY_MONEY*(THISFORM.PAGEFRAME1.PAGE2.TXTF10.VALUE/100),0)
                  			            	  CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='B'      &&底薪
                  	                                 	     REPLACE &FILE_S10..F20 WITH ROUND(MONTH_MONEY*(THISFORM.PAGEFRAME1.PAGE2.TXTF10.VALUE/100),0)
      			                            ENDCASE         					  
		                               ELSE                &&扣稅
       					            DO CASE
       					                  CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='H'           &&時薪
                                                         	     MONEY_NOW=TIME_MONEY*(THISFORM.PAGEFRAME1.PAGE2.TXTF10.VALUE/100)
                                                         	     TAX_MONEY=ROUND(MONEY_NOW*(THISFORM.PAGEFRAME1.PAGE2.TXTF12.VALUE/100),0)	
      			                                 	     REPLACE &FILE_S10..F20 WITH MONEY_NOW-TAX_MONEY     	&&本月薪資所得 					            
       					                  CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='D'           &&日薪
                                                         	     MONEY_NOW=DAY_MONEY*(THISFORM.PAGEFRAME1.PAGE2.TXTF10.VALUE/100)
                                                         	     TAX_MONEY=ROUND(MONEY_NOW*(THISFORM.PAGEFRAME1.PAGE2.TXTF12.VALUE/100),0)
      			                                 	     REPLACE &FILE_S10..F20 WITH MONEY_NOW-TAX_MONEY
                  			            	  CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='B' 	     &&底薪
                  	                               		     MONEY_NOW=MONTH_MONEY*(THISFORM.PAGEFRAME1.PAGE2.TXTF10.VALUE/100)
                  	                               		     TAX_MONEY=ROUND(MONEY_NOW*(THISFORM.PAGEFRAME1.PAGE2.TXTF12.VALUE/100),0)
      			                                 	     REPLACE &FILE_S10..F20 WITH MONEY_NOW-TAX_MONEY  
      			                            ENDCASE  		                         
		                               ENDIF        
		                          ELSE     &&發放金額
		                              IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP12.VALUE=1   &&不扣稅
		                                   REPLACE &FILE_S10..F20 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF11.VALUE   
		                              ELSE
		                                   TAX_MONEY=ROUND(THISFORM.PAGEFRAME1.PAGE2.TXTF11.VALUE*(THISFORM.PAGEFRAME1.PAGE2.TXTF12.VALUE/100),0)
		                                   REPLACE &FILE_S10..F20 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF11.VALUE-TAX_MONEY 
		                              ENDIF    
		                          ENDIF	
		                          REPLACE &FILE_S10..F21 WITH 0	&&基本薪資所得
		                          REPLACE &FILE_S10..F22 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE	&&其他發放日期
			                  REPLACE &FILE_S10..F23 WITH IIF(THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP12.VALUE=2,THISFORM.PAGEFRAME1.PAGE2.TXTF12.VALUE,0)	&&扣稅比率
			                  REPLACE &FILE_S10..F98 WITH 'N'
		                          REPLACE &FILE_S10..F97 WITH 'C'
		              	          REPLACE &FILE_S10..F99 WITH DTOC(DATE())+' '+IIF(SEEK(SYS_OPER,'A01'),A01.F03,'')	
		              	          ****** 若要扣稅則需把扣稅之金額寫入到扣款項目中且扣款類別為N:其他所得發放扣稅額
		              	          IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP12.VALUE<>1
			                       SELECT &FILE_S10
				               APPEND BLANK			                    
				               REPLACE &FILE_S10..F01 WITH S10_TEMP.F01
					       REPLACE &FILE_S10..F02 WITH S10_TEMP.F02
				               REPLACE &FILE_S10..F03 WITH S10_TEMP.F03
				               REPLACE &FILE_S10..F04 WITH S10_TEMP.F04              		               
				               REPLACE &FILE_S10..F24 WITH 'N' 
				               REPLACE &FILE_S10..F25 WITH ALLTRIM(THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE)+'扣稅額'
				               REPLACE &FILE_S10..F26 WITH TAX_MONEY
		                               REPLACE &FILE_S10..F97 WITH 'D'
		                               REPLACE &FILE_S10..F98 WITH 'N'
		              	               REPLACE &FILE_S10..F99 WITH DTOC(DATE())+' '+IIF(SEEK(SYS_OPER,'A01'),A01.F03,'')	  		              	           
                                          ENDIF 
                                   ENDIF       
                                   SELECT S10_TEMP
                                   SKIP              
                             ENDDO     
                             SELECT S10_TEMP
                             USE
	               	     SELECT &FILE_S10
		  	     USE                                 
                        ENDIF 
                   ENDIF   
                  ******不管合併薪資發放為單獨或合併,皆須新增資料至S09A員工其他所得發放作業,且狀態為3:從部門中新增之人員
              	   SELECT F01,F02,F05,F06,F15,F28 FROM S05 WHERE ALLTRIM(F05)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE)  ORDER BY F01 INTO CURSOR S05_TEMP
              	   SELECT S05_TEMP
              	   GO TOP
              	   DO WHILE !EOF() 
              	           SELECT S09		
              	           SET ORDER TO S091 && F01+DTOS(F05)+F06+F16
              	           SEEK S05_TEMP.F01+DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE)+THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE
	              	   IF  !FOUND()  AND IIF( S05_TEMP.F28='1' OR S05_TEMP.F28='3' OR S05_TEMP.F28='7' OR S05_TEMP.F28='8',.F.,.T.)&&判斷該所得於S09A員工其他所得發放作業是否存在相同之發放項目及發放日期
	              	        SELECT S09
	              	        APPEND BLANK
	              	        REPLACE F01 WITH S05_TEMP.F01
		                REPLACE F02 WITH S05_TEMP.F02
			        REPLACE F03 WITH S05_TEMP.F05
			        REPLACE F04 WITH S05_TEMP.F06   	
			        REPLACE F05 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE
			        REPLACE F06 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF06.VALUE
			        REPLACE F07 WITH ALLTRIM(STR(THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP07.VALUE))
			    	IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP07.VALUE=2
			    	     REPLACE F08 WITH CTOD(ALLTRIM(STR(THISFORM.PAGEFRAME1.PAGE2.SPINNER081.VALUE))+'/'+ALLTRIM(STR(THISFORM.PAGEFRAME1.PAGE2.SPINNER082.VALUE))+'/01')
			    	ENDIF  			        
			        IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP09.VALUE=1
			             REPLACE F09 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE
			             REPLACE F10 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF10.VALUE
			        ELSE    
				     REPLACE F11 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF11.VALUE
			        ENDIF    	
			        REPLACE F12 WITH IIF(THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP12.VALUE=2,THISFORM.PAGEFRAME1.PAGE2.TXTF12.VALUE,0) 
				DO CASE
				      CASE S05_TEMP.F15='M'   &&發薪種類(M:月薪 D:日薪 T:時薪)
				                 MONTH_MONEY=IIF(INDEXSEEK(S05_TEMP.F01+'A',.T.,'S06','S061'),S06.F08,0)  					 	 &&底薪
					         DAY_MONEY=IIF(IIF(INDEXSEEK('Z',.T.,'S00','S001'),S00.F02,0)<>0,MONTH_MONEY/S00.F02,0)      &&日薪
					         TIME_MONEY=IIF(IIF(INDEXSEEK('Z',.T.,'S00','S001'),S00.F03,0)<>0,DAY_MONEY/S00.F03,0)       	 &&時薪
				      CASE S05_TEMP.F15='D'     
						 MONTH_MONEY=0
					         DAY_MONEY=IIF(INDEXSEEK(S05_TEMP.F01+'B',.T.,'S06','S061'),S06.F08,0)     
					         TIME_MONEY=IIF(IIF(INDEXSEEK('Z',.T.,'S00','S001'),S00.F03,0)<>0,DAY_MONEY/S00.F03,0)       			  	   
			              CASE S05_TEMP.F15='T'      
					     	 MONTH_MONEY=0
					         DAY_MONEY=0
					         TIME_MONEY=IIF(INDEXSEEK(S05_TEMP.F01+'C',.T.,'S06','S061'),S06.F08,0)
			        ENDCASE					        
		                IF  THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP09.VALUE=1     &&發放底數                	                                          
		                      IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP12.VALUE=1   &&不扣稅
   				           DO CASE 
   				                 CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='H'           &&時薪
                                                            REPLACE F13 WITH ROUND(TIME_MONEY*(S09.F10/100),0)   				           
   				                 CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='D'           &&日薪
                                                            REPLACE F13 WITH ROUND(DAY_MONEY*(S09.F10/100),0)
              			                 CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='B'           &&底薪
              	                                            REPLACE F13 WITH ROUND(MONTH_MONEY*(S09.F10/100),0)
  			                   ENDCASE   
  			                   REPLACE F17 WITH S09.F13     	&&稅後小計				  
	                               ELSE                &&扣稅
   				           DO CASE 
   				                 CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='H'           &&時薪
                                                            MONEY_NOW=ROUND(TIME_MONEY*(S09.F10/100),0)
      			                                    REPLACE F17 WITH MONEY_NOW-ROUND(MONEY_NOW*(S09.F12/100),0)   				                 
   				                 CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='D'           &&日薪
                                                            MONEY_NOW=ROUND(DAY_MONEY*(S09.F10/100),0)
      			                                    REPLACE F17 WITH MONEY_NOW-ROUND(MONEY_NOW*(S09.F12/100),0)   
              			                 CASE THISFORM.PAGEFRAME1.PAGE2.TXTF091.VALUE='B'           &&底薪
                                                            MONEY_NOW=ROUND(MONTH_MONEY*(S09.F10/100),0)
      			                                    REPLACE F17 WITH MONEY_NOW-ROUND(MONEY_NOW*(S09.F12/100),0)   
  			                    ENDCASE   	                               
      			                    REPLACE F13 WITH MONEY_NOW	                         
		                       ENDIF        
		                ELSE     &&發放金額
		                       REPLACE F13 WITH S09.F11
		                       IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP12.VALUE=1   &&不扣稅
		                            REPLACE F17 WITH S09.F11 
		                       ELSE
		                            REPLACE F17 WITH S09.F11-ROUND(S09.F11*(S09.F12/100),0)
		                       ENDIF                      
		                 ENDIF
			         REPLACE F14 WITH THISFORM.PAGEFRAME1.PAGE2.TXTF14.VALUE
			         REPLACE F15 WITH  'Y'    	
			         REPLACE F16 WITH  '3'  	&&3:從部門中新增之人員
			         REPLACE F99 WITH DTOC(DATE())+' '+IIF(SEEK(SYS_OPER,'A01'),A01.F03,'')
			  ENDIF   
			  SELECT S05_TEMP
			  SKIP
              	   ENDDO             	  
              	   SELECT S05_TEMP
              	   USE         	     
              	   SELECT S06
              	   SET ORDER TO S063
                   SELECT S09
                   SET ORDER TO S092  && F03+DTOS(F05)+F06+F16
                   =MESSAGEBOX('此發放作業確認成功!',0+64,'提示訊息視窗')    
                   THISFORM.GRID2.SETFOCUS    
            ENDIF 
        ENDIF   
     ENDIF    
     UNLOCK   ALL 	
     THISFORM.PAGEFRAME1.PAGE2.ACTIVATE 	   
   ENDPROC
