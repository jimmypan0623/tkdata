  PROC ANS_BOTT.CLICK     &&過帳程序  
       &&C05(出貨計劃)->F04,F05
       &&E08(MRP)檔
       &&B01(物料基本資料檔)->總庫存數量(F12),可分配量(F13)
       &&B11(庫存明細檔)->庫存數量(F04),最後使用日期(F08)
       &&B39(未過帳單據)->整筆刪除
       if messagebox('是否確認資料無誤可以過帳?',4+32+256,'請確認') = 6	        
    
          SELE RECNO() FROM B61 WHERE B61.F01=B61_1.F01  AND F12<>'2' INTO ARRAY BK1              
               JJ1=''                
               IF _TALLY>0   
                  FOR I= 1 TO ALEN(BK1)
                      JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
                  ENDFOR    
                  KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')              
                  RLOCK(KK1,'B61')
                  LL1=RLOCK(KK1,'B61')
               ELSE
                  LL1=.T.   
               ENDIF                   
               IF LL1 =.T. 
                  IF LEN(ALLTRIM(JJ1))>0 
                     SELE B61
                     FOR I= 1 TO ALEN(BK1)                      
                         GO BK1(I) 
                         SELE B01                                            &&物料基本主檔
                         SEEK B61.F03
                         IF FOUND()
                            IF !EMPTY(F98)                                   &&如果不是虛擬料號才做庫存運算
                               SELE B11                                            &&部門庫存明細檔 ---->入本廠收料部門
                               SEEK B61.F05+B61.F03+IIF(INT_028,B61.F06,SPACE(5))
                               IF FOUND()
                                  REPL F04 WITH F04+B61.F04
                                  IF F04=0
                                     DELE
                                  ELSE   
                                     IF F05<CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B61.F02))
                                        REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B61.F02))
                                     ENDIF
                                  ENDIF   
                               ELSE
                                  APPEND BLANK
                                  REPL F01 WITH B61.F05
                                  REPL F03 WITH B61.F03
                                  REPL F04 WITH (B61.F04)                                                                    
                                  REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B61.F02))
                                  IF INT_028
                                     REPLACE F08 WITH B61.F06
                                  ENDIF   
                               ENDIF   
                               SEEK LEFT(INT_190,5)+B61.F03+IIF(INT_028,B61.F06,SPACE(5))                     &&部門庫存明細----->借出入部門借出
                               IF FOUND()
                                  REPL F04 WITH F04+B61.F04*(-1)
                                  IF F04=0
                                     DELE
                                  ELSE   
                                     IF F05<CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B61.F02))
                                        REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B61.F02))
                                     ENDIF
                                  ENDIF   
                               ELSE
                                  APPEND BLANK
                                  REPL F01 WITH LEFT(INT_190,5)
                                  REPL F03 WITH B61.F03
                                  REPL F04 WITH (B61.F04)*(-1)                                                                    
                                  REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B61.F02))
                                  IF INT_028
                                     REPLACE F08 WITH B61.F06
                                  ENDIF   
                               ENDIF                                  
                               SELE B25                                            
                               SEEK B61.F05+B61.F03                            &&分部門庫存月報表 ---->本廠收料部門
                               IF FOUND()
                                  REPL F18 WITH F18+B61.F04
                                  REPL F15 WITH F15+(B61.F04)
                               ELSE
                                  APPEND BLANK
                                  REPL F01 WITH B61.F05
                                  REPL F02 WITH B61.F03
                                  REPL F18 WITH B61.F04
                                  REPL F15 WITH (B61.F04)                                                 
                               ENDIF   
                               SEEK LEFT(INT_190,5)+B61.F03                            &&分部門庫存月報表 ---->借出入部門借出
                               IF FOUND()
                                  REPL F20 WITH F20+B61.F04
                                  REPL F15 WITH F15+(B61.F04)*(-1)
                               ELSE
                                  APPEND BLANK
                                  REPL F01 WITH LEFT(INT_190,5)
                                  REPL F02 WITH B61.F03
                                  REPL F20 WITH B61.F04
                                  REPL F15 WITH (B61.F04)*(-1)                                                 
                               ENDIF                                  
                               SELE B26                                            &&庫存異動紀錄
                               APPEND BLANK                                        &&收料部門
                               REPL F01 WITH B61.F03
                               REPL F02 WITH B61.F05
                               REPL F03 WITH B61.F02
                               REPL F04 WITH (B61.F04)
                               REPL F06 WITH 'U'
                               REPL F07 WITH B61.F01
                               REPL F08 WITH '借自'+SPACE(2)+B61.F10+SPACE(1)+IIF(SEEK(B61.F10,'D01'),D01.F04,'')
                               APPEND BLANK                                        &&借出入部門借出
                               REPL F01 WITH B61.F03
                               REPL F02 WITH LEFT(INT_190,5)
                               REPL F03 WITH B61.F02
                               REPL F04 WITH (B61.F04)*(-1)
                               REPL F06 WITH 'U'
                               REPL F07 WITH B61.F01
                               REPL F08 WITH '借自'+SPACE(2)+B61.F10+SPACE(1)+IIF(SEEK(B61.F10,'D01'),D01.F04,'')                                                              
                            ENDIF
                         ENDIF   
                             SELE C05                                            &&出貨計劃
                             SET ORDER TO C054                 
                             APPEND BLANK
                             REPL F01 WITH B61.F01
                             REPL F02 WITH B61.F03
                             REPL F03 WITH B61.F14
                             REPL F04 WITH B61.F04
                             REPL F06 WITH B61.F10
                             REPL F07 WITH IIF(INT_012=1,PADL(ALLTRIM(STR(YEAR(DATE())-1911)),3,' ')+RIGHT(DTOC(DATE()),6),dtoc(DATE())+' ')+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
                             REPL F08 WITH sys_oper
                             REPLACE F15 WITH '0'
                             IF INT_028
                                REPLACE F09 WITH B61.F06+IIF(SEEK(B61.F06,'C01'),C01.F05,'')
                             ELSE
                                REPLACE F09 WITH B61.F08   
                             ENDIF   
                             SELE B61
                             REPL F12 WITH '2'
                             REPL F90 WITH IIF(INT_012=1,PADL(ALLTRIM(STR(YEAR(DATE())-1911)),3,' ')+RIGHT(DTOC(DATE()),6),dtoc(DATE())+' ')+IIF(SEEK(sys_oper,'A01'),A01.F03,'')                            
                    ENDFOR   
                    SELE B39                    &&未過帳庫存單據查詢檔
                    SEEK 'B61'+B61_1.F01
                    IF FOUND()
                       DELE
                    ENDIF                                             
                    SELE B61_1
                    REPL F12 WITH '2'
                    REPL F90 WITH IIF(INT_012=1,PADL(ALLTRIM(STR(YEAR(DATE())-1911)),3,' ')+RIGHT(DTOC(DATE()),6),dtoc(DATE())+' ')+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
                  ELSE
                     =MESSAGEBOX('此單據已由別的工作站過帳完成',0+64,'提示訊息視窗')
                     SELE B61_1
                     REPL F12 WITH '2'
                  ENDIF           
                  THIS.ENABLED=.F.
                  THISFORM.ORPGROUP.EDIT_BOTT.ENABLED=.F.
                  THISFORM.ORPGROUP.DEL_BOTT.ENABLED=.F.             
                  UNLOCK ALL
                  IF THISFORM.PAGEFRAME1.ACTIVEPAGE=1
                     THISFORM.GRID1.SETFOCUS
                  ELSE
                      THISFORM.GRID2.SETFOCUS
                      THISFORM.ORPGROUP.NEW_BOTT.ENABLED=.F.
                  ENDIF
                  THISFORM.REFRESH
