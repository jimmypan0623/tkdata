DEFINE CLASS TEXTBOX1 AS TEXTBOX
  PROCEDURE INTERACTIVECHANGE 
         SELECT OQT
         IF OQT.EXP_1=1
             DO CASE 
                   CASE THIS.VALUE<=0
                    	      THIS.VALUE=(OQT.F08-OQT.F09)
                    	      =MESSAGEBOX('退料數量不得小於等於零,請重新輸入!!',0+64,'提示訊息視窗')        
                  CASE THIS.VALUE>OQT.F08-OQT.F09
			     =MESSAGEBOX('退料數量不得大於'+TRANSFORM(OQT.F08-OQT.F09,'@R 999999999.9999'),0+48,'提示訊息視窗') 
                    	      THIS.VALUE=(OQT.F08-OQT.F09)		                        	      
             ENDCASE   
             REPLACE OQT.E11_F07 WITH THIS.VALUE        
         ENDIF  
         THISFORM.GRID1.SETFOCUS  
   ENDPROC 
