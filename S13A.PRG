GO TOP
FIRST_DATE=LEFT(DTOS(A23.F01),4)+'/'+SUBSTR(DTOS(A23.F01),5,2) &&系統月份檔第一筆開帳月份
GO BOTTOM
LAST_DATE=LEFT(DTOS(A23.F01),4)+'/'+SUBSTR(DTOS(A23.F01),5,2)  &&系統月份檔最後一筆開帳月份
IF !USED('S0A')
   SELE 0
   USE S0A 
ELSE
   SELE S0A
ENDIF
SET ORDER TO S0A1
