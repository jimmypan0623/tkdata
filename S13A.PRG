FLG='0'
FCH=''
CHK=''
STAR_DATE=''
END_DATE=''
IF !USED('A01')
   SELE 0
   USE A01
ELSE 
   SELE A01
ENDIF
SET ORDER TO A01      
IF !USED('A02')
   SELE 0
   USE A02 INDE A02
ELSE
   SELE A02
ENDIF
SET ORDER TO A021      
SEEK SYS_OPER+'S13'    
IF !USED('A23')		&&月份檔
   SELE 0
   USE A23 INDE A23
ELSE
   SELE A23
ENDIF
