**程式名稱:S11.薪資月報表
SET EXCL OFF
CLOSE ALL
CLEAR 
AREA1='S11_TEMP'
FLG='0'
FCH=''
CHK=''
HK=''
IF !USED('A01')	&&操作人員資料設定
   SELECT  0
   USE A01
ELSE 
   SELECT  A01
ENDIF
SET ORDER TO A01    
IF !USED('A23')	&&月份檔
    SELE 0
    USE A23
ELSE 
   SELE A23
ENDIF
SET ORDER TO A231   
IF !USED('S05')  &&人事薪資基本檔
   SELECT  0
   USE S05 
ELSE
   SELECT  S05
ENDIF
SET ORDER TO S051 
IF !USED('A02')	&&權限設定檔
   SELECT  0
   USE A02 INDE A02
ELSE
   SELECT  A02
ENDIF
SET ORDER TO A021      
SEEK SYS_OPER+'S05'   
IF FOUND()  AND A02.F09='*'   &&判斷是否有權限可處理所有員工
    SELECT F01 FROM S05 INTO CURSOR S05_TEMP
ELSE
    SELECT F01 FROM S05 WHERE F27=.T. INTO CURSOR S05_TEMP
ENDIF
SELECT A02  
SEEK SYS_OPER+'S11'   
IF !USED('S11')	&&S11 薪資月報表
   SELECT  0
   USE S11 
ELSE
   SELECT  S11
ENDIF
SET ORDER TO S111
CREATE CURSOR S11_TEMP;
 (F01 D(8),F02 C(4),F03 C(8),F04 C(4),F05 C(8),F06 C(1),F07 N(4,1),F08 N(7),F09 N(7),F10 N(7),;
  F11 N(7),F12 N(7),F13 N(7),F14 N(7),F15 N(7),F16 N(7),F17 N(7),F18 N(7),F19 N(7),F20 N(7),;
  F21 N(7),F22 N(7),F23 N(7),F24 N(7),F25 N(7),F26 N(7),F27 N(7),F28 N(7),F29 N(7),F30 N(7),F31 N(7),F32 N(7),F99 C(17))
INDE ON F02+DTOS(F01) TAG S11_TEMP1
INDE ON F04+DTOS(F01) TAG S11_TEMP2
SELECT S11.* FROM S11,S05_TEMP WHERE  ALLTRIM(S11.F02)=ALLTRIM(S05_TEMP.F01);
            AND LEFT(DTOS(S11.F01),6)=LEFT(DTOS(DATE()),6) INTO CURSOR S11A_TEMP
SELECT S11A_TEMP
GO TOP
DO WHILE !EOF()
      SELECT S11_TEMP 
      APPEND BLANK 
      REPLACE F01 WITH S11A_TEMP.F01		&&薪資日期
      REPLACE F02 WITH S11A_TEMP.F02		&&員工編號
      REPLACE F03 WITH S11A_TEMP.F03		&&員工姓名
      REPLACE F04 WITH S11A_TEMP.F04		&&部門編號
      REPLACE F05 WITH S11A_TEMP.F05		&&部門名稱
      REPLACE F06 WITH S11A_TEMP.F06		&&發薪種類( M:月薪 D:日薪 T:時薪)
      REPLACE F07 WITH S11A_TEMP.F07		&&上班天數或時數
      REPLACE F08 WITH S11A_TEMP.F08		&&遲到額外扣款小計
      REPLACE F09 WITH S11A_TEMP.F09		&&早退額外扣款小計
      REPLACE F10 WITH S11A_TEMP.F10		&&基本薪資所得總計(S05)
      REPLACE F11 WITH S11A_TEMP.F11		&&基本薪資扣款總計(S05)
      REPLACE F12 WITH S11A_TEMP.F12		&&本月基本薪資所得(不包含新增薪資所得)
      REPLACE F13 WITH S11A_TEMP.F13		&&本月新增薪資所得
      REPLACE F14 WITH S11A_TEMP.F14		&&底/日/時  薪金額
      REPLACE F15 WITH S11A_TEMP.F15		&&伙食費
      REPLACE F16 WITH S11A_TEMP.F16		&&交通費
      REPLACE F17 WITH S11A_TEMP.F17		&&全勤獎金
      REPLACE F18 WITH S11A_TEMP.F18		&&全勤扣款
      REPLACE F19 WITH S11A_TEMP.F19		&&職務津貼
      REPLACE F20 WITH S11A_TEMP.F20		&&自設項目金額
      REPLACE F21 WITH S11A_TEMP.F21		&&其他所得發放 
      REPLACE F22 WITH S11A_TEMP.F22		&&加班所得(不包括津貼金額) 
      REPLACE F23 WITH S11A_TEMP.F23		&&加班津貼補助金額
      REPLACE F24 WITH S11A_TEMP.F24		&&勞保費 
      REPLACE F25 WITH S11A_TEMP.F25		&&健保費 
      REPLACE F26 WITH S11A_TEMP.F26		&&所得稅
      REPLACE F27 WITH S11A_TEMP.F27		&&其他扣款 
      REPLACE F28 WITH S11A_TEMP.F28		&&其他所得發放扣稅金額 
      REPLACE F29 WITH S11A_TEMP.F29		&&缺勤扣款 
      REPLACE F30 WITH S11A_TEMP.F30		&&懲處扣款
      REPLACE F31 WITH S11A_TEMP.F31		&&實發薪資    
      REPLACE F32 WITH S11A_TEMP.F32		&&勞退自行提撥金額   
      REPLACE F99 WITH S11A_TEMP.F99		&&安全資料
      SELECT S11A_TEMP
      SKIP
ENDDO
SELECT S11A_TEMP
USE
SELECT S11_TEMP
SET ORDER TO S11_TEMP1
GO TOP
