     DO CASE
        CASE JUKK='A'
             JOO='新進人員'
        CASE JUKK='B'
             JOO='資料補登'     
        CASE JUKK='C'
             JOO='在        職'                       
        CASE JUKK='1'
             JOO='已  離  職'
        CASE JUKK='2'
             JOO='離職復職'
        CASE JUKK='3'
             JOO='留職停薪'           
        CASE JUKK='4'
             JOO='留職復職'            
        CASE JUKK='5'
             JOO='職務調整'       
        CASE JUKK='6'
             JOO='升        遷'                                                             
     ENDCASE
     RETURN JOO  
