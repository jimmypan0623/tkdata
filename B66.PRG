*******************************************   客戶別計算借出總金額
FUNC TTL1
     PARA BKK_NO
     qt_od=0
     sele D07
     SET ORDER TO D073
     SET RELA TO F02 INTO B01
     SEEK BKK_NO
     IF FOUND()
        DO WHILE F05=BKK_NO            
           qt_od=qt_od+f04*B01.F38
           SELE D07
           SKIP
        ENDDO
     ELSE
       qt_od=0
     ENDIF
     SELE B66_1
     RETURN ROUND(qt_od,0)
