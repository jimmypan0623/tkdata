   SELE 0
   USE B01 INDE B01
ELSE
   SELE B01
ENDIF
SET ORDER TO 1   
SELE 0
USE C01
SET ORDER TO 1
SELE 0
IF !USED('D07')
   SELE 0
   USE D07
ELSE
   SELE D07
ENDIF
SET ORDER TO D073
SET FILTER TO LEFT(F01,2)='BW'
CREATE CURSOR B66_1;
(F05 C(5),FL1 N(12))
INDE ON F05 TAG B66_11
SELE F05 FROM D07 GROUP BY F05 ORDER BY F05 INTO CURSOR B66_1A WHERE LEFT(F01,2)='BW'
SELE B66_1A
GO TOP
DO WHILE !EOF()
   SELE B66_1
   APPEND BLANK
   REPL F05 WITH B66_1A.F05  
   REPL FL1 WITH TTL1(B66_1A.F05)
   SELE B66_1A
   SKIP   
ENDDO
SELE B66_1A
USE   
SELE B66_1
SET ORDER TO 1
SET RELA TO F05 INTO C01      
CREATE CURSOR B66_2;
(F02 C(43),FL2 N(12))
INDE ON F02 TAG B66_21
SELE F02 FROM D07 GROUP BY F02 ORDER BY F02 INTO CURSOR B66_2A WHERE LEFT(F01,2)='BW'
SELE B66_2A
GO TOP
DO WHILE !EOF()
   SELE B66_2
   APPEND BLANK
   REPL F02 WITH B66_2A.F02  
   REPL FL2 WITH TTL2(B66_2A.F02)
   SELE B66_2A
   SKIP   
ENDDO
SELE B66_2A
USE 
SELE B66_2
SET ORDER TO 1
CREATE CURSOR B66_3;
(F02 C(43),F04 N(12),F05 C(5))
INDE ON F05+F02 TAG B66_31
INDE ON F02+F05 TAG B66_32
SELE F02,SUM(F04),F05 FROM D07 GROUP BY F05,F02 ORDER BY F05,F02 INTO CURSOR B66_3A WHERE LEFT(F01,2)='BW'
SELE B66_3A
GO TOP
DO WHILE !EOF()
   SELE B66_3
   APPEND BLANK
   REPL F02 WITH B66_3A.F02
   REPL F04 WITH B66_3A.SUM_F04
   REPL F05 WITH B66_3A.F05
   SELE B66_3A
   SKIP
ENDDO
