  PROCEDURE  EXCEL_BOTT.CLICK
       PO_NO=P19_1.F03
       NAME_NO=IIF(SEEK(PO_NO,'D01'),ALLTRIM(D01.F04),'')
       DATE_STR=LEFT(THISFORM.MTH_LIST.DISPLAYVALUE,2)+RIGHT(THISFORM.MTH_LIST.DISPLAYVALUE,2)
       FILE_NAME=DATE_STR+'外購應付帳款對帳單('+NAME_NO+')'
       CREATE CURSOR P19A (F01 D(8),F02 C(10),F04 C(10),F05 C(43),F06 N(11,2),F07 N(14,6),F13 C(8),F08 N(14,3),F14 N(11,6),F17 C(100),F22 D(8))
       SELECT F01,F02,F04,F05,F06,F07,F08,F13,F14,F17,F22  FROM P19 WHERE F03=PO_NO ORDER BY F17 INTO CURSOR P19_TEMP
       SELECT P19_TEMP
       CALC SUM(F08) TO SUM_F08
       GO TOP
       NOTE_LATE=P19_TEMP.F17
       SUM_ACC_F08=0
       DO WHILE !EOF()
	      IF  ALLTRIM(NOTE_LATE)=ALLTRIM(P19_TEMP.F17)
		    SELE P19A
		    APPEND BLANK
		    REPLACE  P19A.F01 WITH P19_TEMP.F01
		    REPLACE  P19A.F02 WITH P19_TEMP.F02
		    REPLACE  P19A.F04 WITH P19_TEMP.F04
		    REPLACE  P19A.F05 WITH P19_TEMP.F05
		    REPLACE  P19A.F06 WITH P19_TEMP.F06
		    REPLACE  P19A.F07 WITH P19_TEMP.F07
		    REPLACE  P19A.F08 WITH P19_TEMP.F08
		    REPLACE  P19A.F13 WITH P19_TEMP.F13
		    REPLACE  P19A.F14 WITH P19_TEMP.F14
		    REPLACE  P19A.F17 WITH P19_TEMP.F17
		    REPLACE  P19A.F22 WITH P19_TEMP.F22
		    SUM_ACC_F08=SUM_ACC_F08+P19_TEMP.F08
	            NOTE_LATE=P19_TEMP.F17
		    SELE P19_TEMP
		    SKIP   
	     ELSE
	            SELECT  P19A
	            APPEND BLANK
	            REPLACE  P19A.F13 WITH '備註小計'
	            REPLACE  P19A.F08 WITH SUM_ACC_F08          
		    SUM_ACC_F08=0		                 
	            SELECT  P19A
	            APPEND BLANK
		    REPLACE  P19A.F01 WITH P19_TEMP.F01
		    REPLACE  P19A.F02 WITH P19_TEMP.F02
		    REPLACE  P19A.F04 WITH P19_TEMP.F04
		    REPLACE  P19A.F05 WITH P19_TEMP.F05
		    REPLACE  P19A.F06 WITH P19_TEMP.F06
		    REPLACE  P19A.F07 WITH P19_TEMP.F07
		    REPLACE  P19A.F08 WITH P19_TEMP.F08
		    REPLACE  P19A.F13 WITH P19_TEMP.F13
		    REPLACE  P19A.F14 WITH P19_TEMP.F14
		    REPLACE  P19A.F17 WITH P19_TEMP.F17
		    REPLACE  P19A.F22 WITH P19_TEMP.F22	
		    SUM_ACC_F08=SUM_ACC_F08+P19_TEMP.F08
	            NOTE_LATE=P19_TEMP.F17
		    SELE P19_TEMP
		    SKIP 	            	                        
	    ENDIF     		    
       ENDDO  
       SELECT P19_TEMP
       USE
       SELE P19A
       APPEND BLANK
       REPLACE  P19A.F13 WITH '備註小計'
       REPLACE  P19A.F08 WITH SUM_ACC_F08       
       SELE P19A
       APPEND BLANK
       APPEND BLANK
       REPLACE  P19A.F13 WITH '總計金額'
       REPLACE  P19A.F08 WITH SUM_F08          
       SELECT F01 日期,F02 進貨單號,F04 採購單號,F05 料號,F06 數量,F07 單價,F14 匯率,F13 幣別,F08 小計,F17 備註,F22 原進貨日 FROM P19A  NOWAIT
       IF _TALLY >0
	   GCDELIMFILE=PUTFILE('儲存路徑',FILE_NAME,'XLS')
	   IF EMPTY(GCDELIMFILE)  && ESC PRESSED
               THISFORM.GRID1.SETFOCUS
               RETURN
           ELSE
                 ON ERROR DO FILE_IMPACT
                 COPY TO (GCDELIMFILE) TYPE XL5   
		 =MESSAGEBOX('儲存成功',0+64,'提示訊息視窗')  
		 SELECT P19A
		 USE
	   ENDIF	
	ELSE
	    =MESSAGEBOX('無資料可轉出',0+64,'提示訊息視窗')                                  
        ENDIF
        THISFORM.GRID1.SETFOCUS
  ENDPROC   
