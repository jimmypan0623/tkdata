  PROC ORPGROUP.DEL_BOTT.CLICK                    &&刪除資料程序
       IF messagebox('是否確定要刪除此筆資料',4+32+256,'請確認') = 6	        
          if thisform.pageframe1.activepage=1          &&刪除整張訂單
             SELE P19_1             
                PO_NO=P19_1.F03
                SELE RECNO() FROM P19 WHERE P19.F03=PO_NO  INTO ARRAY BK1              
                JJ1=''                
                IF _TALLY>0   
                   FOR I= 1 TO ALEN(BK1)
                       JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
                   ENDFOR    
                   KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')              
                   RLOCK(KK1,'P19')
                   LL1=RLOCK(KK1,'P19')
                ELSE
                   LL1=.T.   
                ENDIF                   
                IF LL1 .AND. RLOCK('P19_1')     
                   sele P19    
                   DELE FROM P19 WHERE F03=PO_NO                                   
                   SELE P19_1
                   DELE
                   UNLOCK ALL
                ELSE
                   DO file_impact                  
                ENDIF
             
             THISFORM.GRID1.SETFOCUS
             IF THISFORM.GRID1.ACTIVEROW=0
                THISFORM.CMDGROUP.ENABLED=.F.
                THISFORM.ORPGROUP.EDIT_BOTT.ENABLED=.F.
                THISFORM.ORPGROUP.DEL_BOTT.ENABLED=.F.               
                THISFORM.ORPGROUP.PNT_BOTT.ENABLED=.F.
             ELSE      
                THISFORM.CMDGROUP.ENABLED=.T.
                THISFORM.ORPGROUP.DEL_BOTT.ENABLED=IIF(A02.F06='*',.T.,.F.)    &&判斷有無刪除的權限
                THISFORM.ORPGROUP.PNT_BOTT.ENABLED=IIF(A02.F07='*',.T.,.F.)   &&判斷有無列印的權限
             ENDIF     
          ELSE                                                                &&刪除單筆資料        
              SELE P19         
              RLOCK()                                             
              IF RLOCK()
                 SELE D11
                 DELE FROM D11 WHERE F05+F04+F03=THISFORM.PAGEFRAME1.PAGE2.TXTF04.VALUE+IIF(EMPTY(P19.F18),THISFORM.PAGEFRAME1.PAGE2.TXTF02.VALUE,P19.F18)+THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE   
                 SELECT P19                         
                DO CASE
                      CASE LEFT(P19.F02,2)='PB' AND LEFT(P19.F04,4)<>'稅額'                      &&進貨
                                 IF LEFT(P19.F19,2)<>'02'
                                    REPLACE P19_1.f06 WITH P19_1.F06+P19.F08*(-1)
                                 ELSE
                                    REPLACE P19_1.F06 WITH P19_1.F06+ROUND(P19.f08/(1+INT_002/100),INT_068)*(-1)                 &&二聯式應稅        
                                    REPLACE P19_1.F08 WITH P19_1.F08+(P19.F08-ROUND(P19.f08/(1+INT_002/100),INT_068))*(-1)
                                ENDIF    
                      CASE  LEFT(P19.F02,2)='BB' AND LEFT(P19.F04,4)<>'稅額' AND P19.F21<>'03'                 &&退貨
                                  IF LEFT(P19.F19,2)<>'02'
                                      REPLACE P19_1.F07 WITH P19_1.F07+P19.F08*(-1)
                                  ELSE
                                      REPLACE P19_1.F07 WITH P19_1.F07+ROUND(P19.f08/(1+INT_002/100),INT_068)*(-1)               &&二聯式應稅  
                                      REPLACE P19_1.F08 WITH P19_1.F08+(P19.F08-ROUND(P19.f08/(1+INT_002/100),INT_068))*(-1)                                      
                                  ENDIF    
                      CASE   LEFT(P19.F04,4)='稅額'                                        &&稅額
                                      REPLACE P19_1.F08 WITH P19_1.F08+P19.F08*(-1)
                      CASE  LEFT(P19.F02,2)='BB' AND LEFT(P19.F04,4)<>'稅額' AND P19.F21='03'       &&進貨折讓
                                  IF LEFT(P19.F19,2)<>'02'
                                     REPLACE P19_1.F09 WITH P19_1.F09+P19.F08*(-1)                              
                                  ELSE
                                    REPLACE P19_1.F09 WITH P19_1.F09+ROUND(P19.f08/(1+INT_002/100),INT_068)*(-1)                          &&二聯式應稅
                                    REPLACE P19_1.F08 WITH P19_1.F08+(P19.F08-ROUND(P19.f08/(1+INT_002/100),INT_068))*(-1)                                    
                                  ENDIF    
                 ENDCASE                  
                 SELE P19
                 DELE
                 SKIP -1
                 IF BOF()
                    GO TOP
                 ENDIF   
              ELSE
                 DO file_impact                  
              ENDIF                   
              THISFORM.GRID2.SETFOCUS
              IF THISFORM.GRID2.ACTIVEROW=0         &&如果單身被刪空               
                 SELE P19_1                         &&連表頭也要刪除              
                 DELE                 
                 SKIP -1
                 IF BOF()
                    GO TOP
                 ENDIF   
                 THISFORM.PAGEFRAME1.ACTIVEPAGE=1 
                 THISFORM.REFRESH          
              ENDIF
          ENDIF
       ENDIF
       RELEASE ALL LIKE BK*   
  ENDPROC  
