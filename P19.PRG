*******************************************   計算進貨金額
FUNC TTL1
     PARA BKK_NO
     qt_od=0
     sele P19
     SEEK BKK_NO
     IF FOUND()
         DO WHILE F03=BKK_NO            
                IF LEFT(P19.F02,2)='PB' AND LEFT(P19.F04,4)<>'稅額'
                     IF LEFT(P19.F19,2)<>'02'
                         qt_od=qt_od+f08
                      ELSE
                          qt_od=qt_od+ROUND(f08/(1+INT_002/100),INT_068)                 &&二聯式應稅                    
                      ENDIF    
                ENDIF   
                SKIP
          ENDDO
     ELSE
       qt_od=0
     ENDIF
     SELE P19_1
     RETURN ROUND(qt_od,INT_068)
**********************************************  計算退貨金額
FUNC TTL2
     PARA BKK_NO
     qt_od=0
     sele P19
     SEEK BKK_NO
     IF FOUND()
        DO WHILE F03=BKK_NO            
               IF LEFT(P19.F02,2)='BB' AND  LEFT(P19.F04,4)<>'稅額'  AND LEFT(P19.F21,2)<>'03'
                    IF LEFT(P19.F19,2)<>'02'
                        qt_od=qt_od+f08
                    ELSE
                        qt_od=qt_od+ROUND(f08/(1+INT_002/100),INT_068)               &&二聯式應稅
                    ENDIF
               ENDIF   
               SKIP
        ENDDO
     ELSE
       qt_od=0
     ENDIF
     SELE P19_1
     RETURN ROUND(qt_od,INT_068)
************************************************計算稅額
FUNC TTL3
     PARA BKK_NO
     qt_od=0
     sele P19
     SEEK BKK_NO
     IF FOUND()
         DO WHILE F03=BKK_NO            
                IF LEFT(P19.F04,4)='稅額'                    
                     qt_od=qt_od+f08
                ELSE
                     IF LEFT(P19.F19,2)='02'                   
                          qt_od=qt_od+(F08-ROUND(f08/(1+INT_002/100),INT_068))    &&二聯式應稅
                     ENDIF                                                                                  
                ENDIF   
                SKIP
         ENDDO
     ELSE
       qt_od=0
     ENDIF
     SELE P19_1
     RETURN ROUND(qt_od,INT_068)
**********************************************  計算折讓金額
FUNC TTL4
     PARA BKK_NO
     qt_od=0
     sele P19
     SEEK BKK_NO
     IF FOUND()
        DO WHILE F03=BKK_NO            
               IF LEFT(P19.F02,2)='BB' AND  LEFT(P19.F04,4)<>'稅額'  AND LEFT(P19.F21,2)='03'
                   IF LEFT(P19.F19,2)<>'02'                   
                          qt_od=qt_od+f08
                   ELSE     
                        qt_od=qt_od+ROUND(f08/(1+INT_002/100),INT_068)
                   ENDIF                                   
           ENDIF   
           SKIP
        ENDDO
     ELSE
       qt_od=0
     ENDIF
     SELE P19_1
     RETURN ROUND(qt_od,INT_068)     