 PROC ANS_BOTT.CLICK     &&確認作業   
     IF  MESSAGEBOX('您是否要確認此筆資料!',4+32+256,'請確認')=6  
      	   SELECT S12
      	   LOCK()
      	   IF RLOCK()
                IF S12.F19 = 'Y'
                    =MESSAGEBOX('此筆資料已確認過,請查明後再作確認!',0+64,'提示訊息視窗')
                    THISFORM.GRID2.SETFOCUS                    
                ELSE
                    SELECT S12
                    REPLACE S12.F19 WITH 'Y'
                    REPLACE S12.F99 WITH DTOC(DATE())+' '+IIF(SEEK(SYS_OPER,'A01'),A01.F03,'')   
                    SELECT S05.F01,S05.F02,S05.F05,S05.F06,S05.F11,S05.F12,S05.F15 FROM S05 WHERE EMPTY(S05.F10) AND ALLTRIM(S05.F05) = ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE) ORDER BY F01 INTO CURSOR S05_TEMP
                    IF THISFORM.PAGEFRAME1.PAGE2.COMBOBOX06.VALUE = 1	&&年終獎金發放
                        SELECT S05_TEMP
                        GO TOP
                        DO WHILE !EOF()
                               SELECT S12
                               APPEND BLANK 
                               REPLACE S12.F01 WITH S05_TEMP.F01
