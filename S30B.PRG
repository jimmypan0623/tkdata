  	   RETURN 
      ENDIF      
      CREATE CURSOR S12_TEMP (F03 C(4),F04 C(8),F05 D(8),F06 C(20),F10 C(50),F13 C(4),F18 C(40))
      SELECT * FROM S12 WHERE F03>=THISFORM.TEXT1.VALUE AND F03<=THISFORM.TEXT2.VALUE;
                    AND F20='2'  AND F19='Y' ORDER BY F03,F05 INTO CURSOR S12A
      SELECT S12A
      GO TOP
      DO WHILE !EOF()                    
             SELECT S12_TEMP
             APPEND BLANK 
             REPLACE S12_TEMP.F03 WITH S12A.F03
             REPLACE S12_TEMP.F04 WITH S12A.F04
             REPLACE S12_TEMP.F05 WITH S12A.F05
             REPLACE S12_TEMP.F06 WITH IIF(S12A.F06='1','年終獎金發放','未休年假獎金發放')
             REPLACE S12_TEMP.F13 WITH IIF(S12A.F13='H','時薪',IIF(S12A.F13='D','日薪','底薪'))
             DO CASE 
                    CASE S12A.F06 = '1'
                    	       DO CASE
                    	             CASE S12A.F10 = '1'
                    	                        REPLACE S12_TEMP.F10 WITH '發放基數('+ALLTRIM(TRANSFORM(S12A.F12,'@R 999,999.99'))+') X 發放底數('+S12_TEMP.F13+') X (到職月數/12)'
                    	             CASE S12A.F10 = '2'
                    	                        REPLACE S12_TEMP.F10 WITH '發放基數('+ALLTRIM(TRANSFORM(S12A.F12,'@R 999,999.99'))+') X 發放底數('+S12_TEMP.F13+')'
                    	             CASE S12A.F10 = '3'
                    	                       REPLACE S12_TEMP.F10 WITH '發放基數('+ALLTRIM(TRANSFORM(S12A.F12,'@R 999,999.99'))+') X 發放金額('+ALLTRIM(TRANSFORM(S12A.F14,'@R 999,999,999'))+')'
                    	       ENDCASE
                    CASE S12A.F06 = '2'
                    	       DO CASE
                    	             CASE S12A.F10 = '1'
                    	                        REPLACE S12_TEMP.F10 WITH '未休年假天數'+' X 發放底數('+S12_TEMP.F13+' X 發放比率('+ALLTRIM(TRANSFORM(S12A.F12,'@R 999,999.99'))+')'
                    	             CASE S12A.F10 = '2'
                    	                        REPLACE S12_TEMP.F10 WITH '未休年假天數'+' X 發放底數('+S12_TEMP.F13+')'
                    	             CASE S12A.F10 = '3'
                    	                       REPLACE S12_TEMP.F10 WITH  '未休年假天數'+' X 發放金額('+ALLTRIM(TRANSFORM(S12A.F14,'@R 999,999,999'))+')'
                    	       ENDCASE                    
             ENDCASE
             REPLACE S12_TEMP.F18 WITH S12A.F18
             SELECT S12A
             SKIP
      ENDDO
      SELECT S12A
      USE
      SELECT S12_TEMP
      GO TOP
      IF  _TALLY >0
           REPORT FORM ALLTRIM(INT_116)+'S30B_1' TO PRINT PROMPT   PREVIEW
           SELECT S12_TEMP
           USE
      ELSE
             =MESSAGEBOX('查無此範圍資料',0+48,'警示')
             THISFORM.TEXT1.SETFOCUS
             RETURN     
      ENDIF      
           THISFORM.RELEASE
  ENDPROC           
