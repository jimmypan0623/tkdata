 PROC INVERSE_BOTT.CLICK     &&反確認作業  
      IF  MESSAGEBOX('您是否要反確認此筆資料!',4+32+256,'請確認')=6
      	   SELECT S12
      	   LOCK()
      	   IF RLOCK()
                IF S12.F19 <> 'Y'
                    =MESSAGEBOX('此筆資料尚未確認過,請查明後再作反確認!',0+64,'提示訊息視窗')
                    THISFORM.GRID2.SETFOCUS                    
                ELSE
                    SELECT S12
                    REPLACE S12.F19 WITH 'N'
                    REPLACE S12.F99 WITH DTOC(DATE())+' '+IIF(SEEK(SYS_OPER,'A01'),A01.F03,'')   
                    IF THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP08.VALUE<>1 &&合併薪資
                        S1=LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
          	        FILE_S10='S10'+ALLTRIM(S1)
		        FIND_S10=ALLTRIM(FILE_S10)+'.DBF'
		        S102='S102'+LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
		        S103='S103'+LEFT(DTOS(THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE),6)
		        IF  FILE('&FIND_S10')=.T.   
		              IF  !USED('&FIND_S10')
		                   SELE 0
			           USE &FIND_S10 
		              ELSE
			           SELE &FILE_S10
		              ENDIF  
		              SELECT &FILE_S10
		              SET ORDER TO S102   && F01+F97+F18+F19+DTOS(F22)                     
		              ****找出未確認且已新增C.薪資所得明細
                              SELECT * FROM &FILE_S10 WHERE ALLTRIM(&FILE_S10..F03)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE);
 	                                     AND ALLTRIM(&FILE_S10..F19)= IIF(S12.F06='1','年終獎金發放','未休年假獎金發放') AND &FILE_S10..F22=THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE;
 	                                     AND  &FILE_S10..F18='I'  AND &FILE_S10..F97='C'  AND &FILE_S10..F98='N' INTO CURSOR S10C
 	                      SELECT S10C
 	                      GO TOP
 	                      DO WHILE !EOF()	              
 	                             SELECT &FILE_S10
 	                             SEEK S10C.F01+'C'+'I'+S10C.F19+DTOS(S10C.F22)
 	                             IF FOUND()
 	                                 DELETE 
 	                             ENDIF
 	                             SELECT S10C
 	                             SKIP
 	                      ENDDO
 	                      SELECT S10C
 	                      USE  
		              *****找出D.薪資扣款明細中之其他所得扣稅額
		              SELECT &FILE_S10
		              SET ORDER TO  S103 && F01+F97+F24+F25
                              SELECT  * FROM &FILE_S10 WHERE ALLTRIM(&FILE_S10..F03)=ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE);
 	                                     AND ALLTRIM(&FILE_S10..F25)=ALLTRIM(IIF(S12.F06='1','年終獎金發放','未休年假獎金發放')+'扣稅額');
 	                                     AND  &FILE_S10..F24='N'  AND &FILE_S10..F97='D'  AND &FILE_S10..F98='N' INTO CURSOR S10D
 	                      SELECT S10D
 	                      GO TOP
 	                      DO WHILE !EOF()	              
 	                             SELECT &FILE_S10
 	                             SEEK S10D.F01+'D'+'N'+S10D.F25
 	                             IF FOUND()
 	                                 DELETE 
 	                             ENDIF
 	                             SELECT S10D
 	                             SKIP
 	                      ENDDO
			      SELECT &FILE_S10
			      USE 	                      
 	                      SELECT S10D
 	                      USE
			ENDIF         
 	            ENDIF                         
                    *****刪除S30A.員工特殊其他所得發放作業中從部門新增之資料
                    SELECT RECNO() FROM S12 WHERE ALLTRIM(S12.F03) = ALLTRIM(THISFORM.PAGEFRAME1.PAGE1.TXTF01.VALUE) AND S12.F05 =  THISFORM.PAGEFRAME1.PAGE2.TXTF05.VALUE ;
                                   AND S12.F06 = ALLTRIM(STR(THISFORM.PAGEFRAME1.PAGE2.COMBOBOX06.VALUE)) AND S12.F08 =  ALLTRIM(STR(THISFORM.PAGEFRAME1.PAGE2.OPTIONGROUP08.VALUE)) ;
                                   AND S12.F19 = 'Y' AND S12.F20 = '3'   INTO ARRAY BK1
		   JJ1=''                
	           IF _TALLY>0   
	               FOR I= 1 TO ALEN(BK1)
	                       JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
	               ENDFOR    
	               KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')              
	               RLOCK(KK1,'S12')
	               LL1=RLOCK(KK1,'S12')
	           ELSE
	               LL1=.T.   
	           ENDIF   
	           IF LL1=.T. AND LEN(ALLTRIM(JJ1))>0  
	               SELECT S12
	               FOR I= 1 TO ALEN(BK1)        
	                       GO BK1(I)   
	                       DELETE 
	               ENDFOR 
	           ENDIF                                          
                    =MESSAGEBOX('此發放作業反確認成功!',0+64,'提示訊息視窗')    
                    THISFORM.GRID2.SETFOCUS 
                ENDIF                  
           ENDIF   
     ENDIF    
     RELEASE ALL LIKE BK* 
     UNLOCK   ALL 	
     THISFORM.PAGEFRAME1.PAGE2.ACTIVATE 	
 ENDPROC
