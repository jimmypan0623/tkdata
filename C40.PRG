  PROC ANS_BOTT.CLICK     &&過帳程序
       IF MESSAGEBOX('是否確認資料無誤可以過帳?',4+32+256,'請確認') = 6	 
          SELE RECNO() FROM C40 WHERE C40.F01=C40_1.F01  AND  C40.F09<>'2'  INTO ARRAY BK1              
          JJ1=''                
          IF _TALLY>0   
             FOR I= 1 TO ALEN(BK1)
                 JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
             ENDFOR    
             KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')              
             RLOCK(KK1,'C40')
             LL1=RLOCK(KK1,'C40')
          ELSE
             LL1=.T.   
          ENDIF  
       ********   
          IF LL1 =.T. 
             IF LEN(ALLTRIM(JJ1))>0 
                SELE C40
                FOR I= 1 TO ALEN(BK1)                      
                    GO BK1(I) 
                    SELE B11                                            &&部門庫存明細檔 
                    SEEK C40.F06+C40.F03+IIF(INT_028,C40.F05,SPACE(5))                                &&轉入部門庫存明細 
                    IF FOUND()
                       REPL F04 WITH F04+C40.F04
                       IF F04=0
                          DELE
                       ELSE   
                          IF F05<CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+C40.F02))
                             REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+C40.F02))
                          ENDIF
                       ENDIF   
                    ELSE
                       APPEND BLANK
                       REPL F01 WITH C40.F06
                       REPL F03 WITH C40.F03
                       REPL F04 WITH C40.F04
                       REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+C40.F02))
                       IF INT_028
                          REPLACE F08 WITH C40.F05
                       ENDIF
                    ENDIF   
                    *******
                    SELE B25                                             &&分部門庫存月報表 
                    SEEK C40.F06+C40.F03
                    IF FOUND()                                                &&轉入部門報表
                       REPL F04 WITH F04+C40.F04		&&進貨數量
                       REPL F15 WITH F15+C40.F04		&&期末數量
                    ELSE
                       APPEND BLANK
                       REPL F01 WITH C40.F06
                       REPL F02 WITH C40.F03
                       REPL F04 WITH C40.F04
                       REPL F15 WITH C40.F04
                    ENDIF   
                    SELE B26                                            &&庫存異動紀錄
                    APPEND BLANK                                               &&轉入  
                    REPL F01 WITH C40.F03                               
                    REPL F02 WITH C40.F06
                    REPL F03 WITH C40.F02
                    REPL F04 WITH C40.F04
                    REPL F06 WITH 'G'
                    REPL F07 WITH C40.F01           
                    REPL F08 WITH '供料客戶 '+C40.F05+SPACE(2)+IIF(SEEK(C40.F05,'C01'),C01.F05,'') 
                    SELE C42                                         &&客供品進貨紀錄檔
                    APPEND BLANK
                    REPL F01 WITH C40.F05
                    REPL F02 WITH C40.F03
                    REPL F03 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+C40.F02))
                    REPL F04 WITH C40.F01
                    REPL F05 WITH C40.F04        
                    SELE C40
                    REPL F90 WITH IIF(INT_012=1,PADL(ALLTRIM(STR(YEAR(DATE())-1911)),3,' ')+RIGHT(DTOC(DATE()),6),dtoc(DATE())+' ')+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
                    REPL F09 WITH '2'
                 ENDFOR   
                 SELE B39                    &&未過帳庫存單據查詢檔
                 SEEK A02.F03+C40_1.F01
                  IF FOUND()
                     DELE
                  ENDIF                                             
                  SELE C40_1
                  REPL F09 WITH '2'
                  REPL F90 WITH IIF(INT_012=1,PADL(ALLTRIM(STR(YEAR(DATE())-1911)),3,' ')+RIGHT(DTOC(DATE()),6),dtoc(DATE())+' ')+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
             ELSE
                =MESSAGEBOX('此單據已由別的工作站過帳完成',0+64,'提示訊息視窗')
                SELE C40_1
                REPL F09 WITH '2'
             ENDIF           
             THIS.ENABLED=.F.
             THISFORM.ORPGROUP.EDIT_BOTT.ENABLED=.F.
             THISFORM.ORPGROUP.DEL_BOTT.ENABLED=.F.             
             UNLOCK ALL
             IF THISFORM.PAGEFRAME1.ACTIVEPAGE=1
                THISFORM.GRID1.SETFOCUS
             ELSE
                 THISFORM.GRID2.SETFOCUS
                 THISFORM.ORPGROUP.NEW_BOTT.ENABLED=.F.
             ENDIF
             THISFORM.REFRESH
          ELSE
            DO FILE_IMPACT
            UNLOCK ALL
            RETURN
          ENDIF   
          UNLOCK ALL
         RELEASE ALL LIKE BK*
      ENDIF      
  ENDPROC
