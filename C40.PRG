  PROC MTH_LIST.INTERACTIVECHANGE
       THISFORM.GRID1.RECORDSOURCE=''
       THISFORM.GRID2.RECORDSOURCE=''
       THISFORM.GRID2.LINKMASTER=''
       THISFORM.GRID2.RELATIONALEXPR=''
       THISFORM.GRID2.CHILDORDER=''
       SELE A24
       USE
       SELECT A23
       SEEK DTOS(CTOD(THIS.DISPLAYVALUE+'/01'))
       A24='A24'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       IF !USED('&A24')
          SELE 0
          USE (A24) ALIA A24
       ELSE
          SELE A24
       ENDIF
       SET ORDER TO 1             
       SELE B25
       USE
       B25='B25'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       IF !USED('&B25')
          SELE 0
          USE (B25) ALIA B25
       ELSE
          SELE B25
       ENDIF
       SET ORDER TO 1                    
       SELE B26
       USE
       B26='B26'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       IF !USED('&B26')
          SELE 0
          USE (B26) ALIA B26
       ELSE
          SELE B26
       ENDIF
       SET ORDER TO 1                           
       SELE B39
       USE
       B39='B39'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       IF !USED('&B39')
          SELE 0
          USE (B39) ALIA B39
       ELSE
          SELE B39
       ENDIF
       SET ORDER TO 1                                  
       SELE C40
       SET RELA OFF INTO B01
       USE
       SELE C40_1
       SET RELA OFF INTO A14 
       SET RELA OFF INTO C01   
       USE
       C40='C40'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       C401='C401'+LEFT(DTOS(CTOD(THIS.DISPLAYVALUE+'/01')),6)
       IF !USED('&C40')
          SELE 0
          USE (C40) ALIA C40
       ELSE
          SELE C40
       ENDIF
       SET ORDER TO 1      
       SET RELA TO F03 INTO B01       
       CREATE CURSOR C40_1;
		(F01 C(10),F02 C(2),F05 C(5),F06 C(5),F07 C(20),F08 C(17),F09 C(1),F90 C(17))
		INDE ON F01 TAG C40_11
		INDE ON F05 TAG C40_12
		INDE ON F06 TAG C40_13
		SELE F01,F02,F05,F06,F07,F08,F09,F90 FROM C40 GROUP BY F01 ORDER BY F01 INTO CURSOR C40_2
		SELE C40_2
		GO TOP
		DO WHILE !EOF()
		   SELE C40_1
		   APPEND BLANK
		   REPL F01 WITH C40_2.F01
		   REPL F02 WITH C40_2.F02
		   REPL F05 WITH C40_2.F05
		   REPL F06 WITH C40_2.F06
		   REPL F07 WITH C40_2.F07
		   REPL F08 WITH C40_2.F08
		   REPL F09 WITH C40_2.F09
		   REPL F90 WITH C40_2.F90
		   SELE C40_2
		   SKIP   
		ENDDO
		SELE C40_2
		USE   
		SELE C40_1
		SET ORDER TO 1
		SET RELA TO F06 INTO A14 
		SET RELA TO F05 INTO C01 ADDI   
       THISFORM.GRID1.RECORDSOURCE='C40_1'
