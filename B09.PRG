  PROC ANS_BOTT.CLICK     &&過帳程序
       IF MESSAGEBOX('是否確認資料無誤可以過帳?',4+32+256,'請確認') = 6	 
           P0_NO=B09_1.F01
           IF !USED('B22')
	        SELECT  0
	        USE B22 
	   ELSE
	        SELE B22
	   ENDIF
	   SET ORDER TO B222                
           SELECT  RECNO() FROM B09 WHERE B09.F01=P0_NO  AND B09.F11<>'2' INTO ARRAY BK1              
           JJ1=''                
           IF _TALLY>0   
               FOR I= 1 TO ALEN(BK1)
                       JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
               ENDFOR    
               KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')              
               RLOCK(KK1,'B09')
               LL1=RLOCK(KK1,'B09')
           ELSE
               LL1=.T.   
           ENDIF                   
           IF LL1=.T. 
               IF LEN(ALLTRIM(JJ1))>0 
                   SELECT  B09
                   FOR I= 1 TO ALEN(BK1)                      
                           GO BK1(I) 
                           SELECT  B11                                            &&部門庫存明細檔 
                           SEEK B09.F05+B09.F03+B09.F06           &&轉出部門庫存明細 
                           IF FOUND()
                               REPLACE  F04 WITH F04+B09.F04
                               B11_F04=B11.F04
                               IF F04=0
                                   DELETE 
                               ELSE   
                                   IF F05<CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B09.F02))
                                       REPLACE  F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B09.F02))
                                  ENDIF
                               ENDIF   
                           ELSE
                               APPEND BLANK
                               REPLACE  F01 WITH B09.F05
                               REPLACE  F03 WITH B09.F03
                               REPLACE  F04 WITH B09.F04
                               REPLACE  F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B09.F02))
                               IF INT_028
                                  REPLACE F08 WITH B09.F06
                               ENDIF   
                               B11_F04=B11.F04
                           ENDIF   
                          *******
                           SELECT  B25                                             &&分部門庫存月報表 
                           SEEK B09.F05+B09.F03
                           IF FOUND()                                                     &&轉出部門報表
                               REPLACE  F13 WITH F13+B09.F04
                               REPLACE  F15 WITH F15+B09.F04
                           ELSE
                               APPEND BLANK
                               REPLACE  F01 WITH B09.F05
                               REPLACE  F02 WITH B09.F03
                               REPLACE  F13 WITH B09.F04
                               REPLACE  F15 WITH B09.F04
                           ENDIF   
                           SELECT  B26                                            &&庫存異動紀錄
                           APPEND BLANK                                               &&轉出  
                           REPLACE  F01 WITH B09.F03                               
                           REPLACE  F02 WITH B09.F05
                           REPLACE  F03 WITH B09.F02
                           REPLACE  F04 WITH B09.F04
                           REPLACE  F06 WITH 'H'
                           REPLACE  F07 WITH B09.F01
                           REPLACE  F08 WITH B09.F07
                           **查看B22.盤點差異表是否有該筆料號且尚未過帳
                           SELECT B22.F01,B22.F02	FROM B22 WHERE ALLTRIM(SUBSTR(B22.F01,6,5))=ALLTRIM(B09.F05) AND ALLTRIM(B22.F02)=ALLTRIM(B09.F03) AND B22.F06<>'2' INTO CURSOR B22_TEMP	
                           SELECT B22_TEMP
                           GO TOP
                           DO WHILE !EOF()
                                 SELECT B22
                                 SEEK B22_TEMP.F01+B22_TEMP.F02
                                 IF FOUND()
                                      REPLACE B22.F03 WITH B11_F04		&&庫存數量
                                      REPLACE B22.F05 WITH B22.F04-B22.F03	&&差異數量=實盤數量-庫存數量
                                 ENDIF
                                 SELECT B22_TEMP
                                 SKIP
                           ENDDO
                           SELECT B22_TEMP
                           USE
                           SELECT  B09
                           REPLACE  F11 WITH '2'
                           REPLACE  F90 WITH IIF(INT_012=1,PADL(ALLTRIM(STR(YEAR(DATE())-1911)),3,' ')+RIGHT(DTOC(DATE()),6),dtoc(DATE())+' ')+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
                   ENDFOR   
                   SELECT   B39                    &&未過帳庫存單據查詢檔
                   SEEK A02.F03+B09_1.F01
                   IF FOUND()
                       DELETE 
                   ENDIF                                                           
                   SELECT  B09_1
                   REPLACE  F11 WITH '2'
                   REPLACE  F90 WITH IIF(INT_012=1,PADL(ALLTRIM(STR(YEAR(DATE())-1911)),3,' ')+RIGHT(DTOC(DATE()),6),dtoc(DATE())+' ')+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
              ELSE
                   =MESSAGEBOX('此單據已由別的工作站過帳完成',0+64,'提示訊息視窗')
                   SELECT  B09_1
                   REPLACE  F11 WITH '2'
              ENDIF           
              THIS.ENABLED=.F.
              THISFORM.ORPGROUP.EDIT_BOTT.ENABLED=.F.
              THISFORM.ORPGROUP.DEL_BOTT.ENABLED=.F.             
              UNLOCK ALL
              IF THISFORM.PAGEFRAME1.ACTIVEPAGE=1
                  THISFORM.GRID1.SETFOCUS
              ELSE
                  THISFORM.GRID2.SETFOCUS
                  THISFORM.ORPGROUP.NEW_BOTT.ENABLED=.F.
              ENDIF
              THISFORM.REFRESH
          ELSE
               DO FILE_IMPACT
               UNLOCK ALL
               RETURN
          ENDIF
          SELECT B22
          USE   
          UNLOCK ALL
          RELEASE ALL LIKE BK*
      ENDIF      
  ENDPROC
