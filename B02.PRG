DEFINE CLASS TEXTBOX1 AS TEXTBOX
  PROCEDURE INTERACTIVECHANGE 
         SELECT OQT
         IF OQT.EXP_1=1
             DO CASE 
                   CASE THIS.VALUE<=0
                    	      THIS.VALUE=OQT.F04-OQT.F06
                    	      =MESSAGEBOX('計價數量不得小於等於零,請重新輸入!!',0+64,'提示訊息視窗')        
                  CASE THIS.VALUE>IIF(SEEK(OQT.F01+OQT.F02,'D04'),D04.F09-D04.F18,0)
                             =MESSAGEBOX('計價數量不得大於未進貨數量'+ALLTRIM(STR(D04.F09-D04.F18)),0+48,'提示訊息視窗')
                    	      THIS.VALUE=OQT.F04-OQT.F06		                        	                      
             ENDCASE   
             REPLACE OQT.D07_F04 WITH THIS.VALUE        
         ENDIF  
         THISFORM.GRID1.SETFOCUS  
   ENDPROC 
ENDDEFINE         

