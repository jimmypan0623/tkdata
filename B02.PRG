  PROC ANS_BOTT.CLICK     &&過帳程序
    if messagebox('是否確認資料無誤可以過帳?',4+32+256,'請確認') = 6	        
       ORGDTE=CTOD('')
       SELE RECNO() FROM B02 WHERE B02.F01=B02_1.F01  AND F10<>'2' INTO ARRAY BK1              
            JJ1=''                
            IF _TALLY>0   
               FOR I= 1 TO ALEN(BK1)
                   JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
               ENDFOR    
               KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')              
               RLOCK(KK1,'B02')
               LL1=RLOCK(KK1,'B02')
            ELSE
               LL1=.T.   
            ENDIF                   
            SELE RECNO() FROM D07 WHERE F01+F02= ANY (SELE F09+F03 FROM B02 WHERE F01=B02_1.F01) INTO ARRAY BK2
            JJ2=''                
            IF _TALLY>0                   
               FOR I= 1 TO ALEN(BK2)
                   JJ2=JJ2+ALLTRIM(STR(BK2(I)))+','
                ENDFOR  
                KK2=STUFF(JJ2,RAT(',',JJ2,1),1,'')
                RLOCK(KK2,'D07')
                LL2=RLOCK(KK2,'D07')
            ELSE
               LL2=.T.   
            ENDIF   
*!*	            SELE RECNO() FROM D04 WHERE F01+F02=ANY (SELE F09+F03 FROM B02 WHERE F01=B02_1.F01) INTO ARRAY BK3
*!*	            JJ3=''                
*!*	            IF _TALLY>0   
*!*	               FOR I= 1 TO ALEN(BK3)
*!*	                   JJ3=JJ3+ALLTRIM(STR(BK3(I)))+','
*!*	               ENDFOR    
*!*	               KK3=STUFF(JJ3,RAT(',',JJ3,1),1,'')              
*!*	               RLOCK(KK3,'D04')
*!*	               LL3=RLOCK(KK3,'D04')
*!*	            ELSE
               LL3=.T.   
*!*	            ENDIF                   
*!*	            SELE RECNO() FROM D03 WHERE F02=ANY (SELE F09 FROM B02 WHERE F01=B02_1.F01) INTO ARRAY BK4
*!*	            JJ4=''
*!*	            IF _TALLY>0   
*!*	               FOR I= 1 TO ALEN(BK4)
*!*	                   JJ4=JJ4+ALLTRIM(STR(BK4(I)))+','
*!*	               ENDFOR    
*!*	               KK4=STUFF(JJ4,RAT(',',JJ4,1),1,'')              
*!*	               RLOCK(KK4,'D03')
*!*	               LL4=RLOCK(KK4,'D03')
*!*	            ELSE
               LL4=.T.   
*!*	            ENDIF     
            SELE RECNO() FROM D01 WHERE F01=B02_1.F07 INTO ARRAY BK5
            JJ5=''
            IF _TALLY>0   
               FOR I= 1 TO ALEN(BK5)
                   JJ5=JJ5+ALLTRIM(STR(BK5(I)))+','
               ENDFOR    
               KK5=STUFF(JJ5,RAT(',',JJ5,1),1,'')              
               RLOCK(KK5,'D01')
               LL5=RLOCK(KK5,'D01')
            ELSE
               LL5=.T.   
            ENDIF                   
            IF LL1 .AND. LL2 .AND. LL3 .AND. LL4 .AND. LL5 =.T. 
               IF LEN(ALLTRIM(JJ1))>0 
                  IF !USED('E08')
                     SELECT 0
                     USE E08
                  ELSE
                     SELECT E08
                  ENDIF
                  SET ORDER TO E083      
                  SELE B02
                  TQTY=0
                  FTTL=0
                  FOR I= 1 TO ALEN(BK1)                      
                      GO BK1(I) 
                      IF B02.F13<>'Y'                               &&如果不需要檢驗才直接做庫存運算                                           
                         TQTY=TQTY+ROUND(IIF(F18=1,F04*F17,F04*F17*F18),INT_068)
                         FTTL=FTTL+ROUND(F04*F17,2)                          
                         SELE B01                                            &&物料基本主檔
                         SEEK B02.F03
                         IF FOUND()                                          &&物料基本主檔存在才做計算
                            IF !EMPTY(F98)                                   &&如果不是虛擬料號才做庫存運算                     
                               SELE B11                                            &&部門庫存明細檔 
                               SEEK B02.F05+B02.F03+IIF(INT_028 AND B01.F98<>'A  ',IIF(SEEK(B02.F09+B02.F03,'D07','D074'),D07.F16,SPACE(5)),SPACE(5))
                               IF FOUND()
                                  REPL F04 WITH F04+(B02.F04+B02.F15)
                                  IF F04=0
                                     DELE
                                  ELSE   
                                     IF F05<CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02))
                                        REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02))
                                        REPL F06 WITH B02.F08
                                     ENDIF
                                  ENDIF   
                               ELSE
                                  APPEND BLANK
                                  REPL F01 WITH B02.F05
                                  REPL F03 WITH B02.F03
                                  REPL F04 WITH (B02.F04+B02.F15)
                                  REPL F05 WITH CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02)
                                  REPL F06 WITH B02.F08                                  
                                  IF INT_028 AND B01.F98<>'A  '
                                     REPLACE F08 WITH IIF(SEEK(B02.F09+B022.F03,'D07','D074'),D07.F16,SPACE(5))
                                  ENDIF
                               ENDIF   
                               SELE B25                                             &&分部門庫存月報表 
                               SEEK B02.F05+B02.F03
                               IF FOUND()
                                  REPL F04 WITH F04+B02.F04+B02.F15
                                  REPL F15 WITH F15+(B02.F04+B02.F15)
                               ELSE
                                  APPEND BLANK
                                  REPL F01 WITH B02.F05
                                  REPL F02 WITH B02.F03
                                  REPL F04 WITH B02.F04+B02.F15
                                  REPL F15 WITH (B02.F04+B02.F15)                                                  
                               ENDIF   
                               SELE B26                                            &&庫存異動紀錄
                               APPEND BLANK
                               REPL F01 WITH B02.F03
                               REPL F02 WITH B02.F05
                               REPL F03 WITH B02.F02
                               REPL F04 WITH (B02.F04+B02.F15)
                               REPL F06 WITH 'A'
                               REPL F07 WITH B02.F01
                               REPL F08 WITH '採'+B02.F09+IIF(EMPTY(B02.F08),'','請'+B02.F08)                              
                            ENDIF
                         ENDIF   
                         TRANS_STYLE=''
                         SELE D07                                            &&進貨計劃
                         SET ORDER TO D074                 
                         SEEK B02.F09+B02.F03
                         IF FOUND()
                            ORGDTE=D07.F03
                            QTY=B02.F04
                            TRANS_STYLE=ALLTRIM(D07.F13)   &&運貨方式
                            CUS_NO=D07.F16       &&客戶編號
                            DO WHILE F01+F02=B02.F09+B02.F03 
                               DO CASE
                                  CASE QTY=F06
                                       SELE D07
                                       REPL F04 WITH F04+F06*(-1)
                                       REPL F06 WITH F06+QTY*(-1)
                                       IF F04=0
                                          DELE
                                       ENDIF   
                                       EXIT
                                  CASE QTY>F06 
                                       SELE D07                              
                                       QTY=QTY+F06*(-1)
                                       DELE     
                                       SKIP
                                  CASE QTY<F06
                                       SELE D07                               
                                       REPL F04 WITH F04+QTY*(-1)
                                       REPL F06 WITH F06+QTY*(-1)
                                       EXIT
                               ENDCASE    
                            ENDDO         
                            SELECT E08
                            SEEK B02.F03
                            IF FOUND()
                               REPLACE F20 WITH F20+B02.F04*(-1)
                            ELSE
                               APPEND BLANK
                               REPLACE F01 WITH CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02)
                               REPLACE F02 WITH B02.F03
                               REPLACE F20 WITH B02.F04*(-1)
                            ENDIF                         
                         ENDIF                                 
                         SELE D10         &&進貨紀錄
                         APPEND BLANK
                         REPL F01 WITH B02.F09
                         REPL F02 WITH B02.F03
                         REPL F03 WITH CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02)
                         REPL F04 WITH B02.F01
                         REPL F05 WITH B02.F04+B02.F15
                         SELE D11         &&進貨日報表
                         APPEND BLANK
                         REPL D11.F01 WITH B02.F02
                         REPL D11.F02 WITH B02.F07
                         REPL D11.F03 WITH B02.F03
                         REPL D11.F04 WITH B02.F01
                         REPL D11.F05 WITH B02.F09
                         REPL D11.F06 WITH INT_011
                         REPL D11.F07 WITH IIF(B02.F18=1,B02.F04*B02.F17,B02.F04*B02.F17*B02.F18)/(B02.F04+B02.F15)
                         REPL D11.F08 WITH B02.F04
                         REPLACE F09 WITH CUS_NO
                         REPLACE F10 WITH IIF(SEEK(F09,'C01','C011'),C01.F05,'')
                         IF ALLTRIM(INT_209) = 'A'
                              REPL D11.F12 WITH IIF(EMPTY(D11.F09),'',IIF(INDEXSEEK(D11.F09,.T.,'C01','C011'),C01.F18,''))
                         ELSE
                              REPL D11.F12 WITH IIF(EMPTY(D11.F09),'',IIF(INDEXSEEK(D11.F09,.T.,'C01','C011'),C01.F33,''))
                         ENDIF
                         REPL D11.F11 WITH IIF(SEEK(D11.F12,'A01'),A01.F03,'')
                         REPL D11.F13 WITH ALLTRIM(TRANS_STYLE)                         
                         IF B02.F02<=IIF(SEEK(B02.F07,'D01'),D01.F15,'')
                            SELE D19         &&鷹付帳款明細
                         ELSE
                            SELE D1I       &&鷹付帳款到下個月或暫存檔
                         ENDIF      
                         APPEND BLANK
                         REPL F01 WITH CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02)
                         REPL F02 WITH B02.F01
                         REPL F03 WITH B02.F07
                         REPL F04 WITH B02.F09
                         REPL F05 WITH B02.F03
                         REPL F06 WITH B02.F04+B02.F15
                         REPL F07 WITH (B02.F04*B02.F17)/(B02.F04+B02.F15)                                                  
                         REPL F08 WITH ROUND(IIF(B02.F18=1,B02.F04*B02.F17,B02.F04*B02.F17*B02.F18),INT_068)
                         REPL F09 WITH IIF(SEEK(B02.F07,'D01'),D01.F13,'1')
                         REPL F12 WITH DTCX(DATE())+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
                         REPL F13 WITH B02.F16
                         REPL F14 WITH B02.F18
                         REPL F17 WITH IIF(!EMPTY(B02.F21),'備註:'+B02.F21,'')+IIF(LEN(ALLTRIM(B02.F20))=10,'發票:'+B02.F20,'')
                         REPLACE F19 WITH ICASE(B02.F23='2','00',B02.F23='3','09',B02.F23='1' AND B02.F22='21','03',B02.F23='1' AND B02.F22='22','02','06')                          
                         REPL F20 WITH B02_1.F20
                         REPL F22 WITH ORGDTE
                         SELE D04           &&訂單表身
                         SEEK B02.F09+B02.F03
                         IF FOUND()
                            REPL F08 WITH F08+B02.F04
                            REPL F09 WITH F09+(B02.F04)*(-1)
                            REPL F18 WITH F18+(B02.F04)*(-1)
                         ENDIF    
                         SELE D03          &&訂單表頭
                         SEEK B02.F09
                         IF FOUND()
                            IF F08<>.T. 
                               REPL F08 WITH .T.
                            ENDIF   
                         ENDIF 
                      ELSE                                     &&需要檢驗
                         ORGDTE=IIF(SEEK(B02.F09+B02.F03,'D07','D074'),D07.F03,CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02))                      
                         SELE H01
                         APPEND BLANK
                         REPLACE F01 WITH B02.F03
                         REPLACE F03 WITH B02.F01
                         REPLACE F04 WITH B02.F07
                         REPLACE F06 WITH B02.F04+B02.F15
                         REPLACE F10 WITH CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02)
                         REPLACE F11 WITH B02.F09
                         REPLACE F12 WITH B02.F08
                         REPLACE F13 WITH B02.F15
                         REPLACE F15 WITH B02.F16
                         REPLACE F16 WITH (B02.F04*B02.F17)/(B02.F04+B02.F15)
                         REPLACE F17 WITH B02.F18
                         REPLACE F19 WITH B02.F05
                         REPLACE F20 WITH ORGDTE                     
                      ENDIF                                  
                      SELE B02
                      CHK_NAME=B02.F12
                      REPL F10 WITH '2'
                      REPL F90 WITH DTCX(DATE())+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
                      REPL F19 WITH ORGDTE
                 ENDFOR   
                 IF LEN(ALLTRIM(B02_1.F20))=10  AND TQTY>0  AND INT_057=.T.    &&如果有發票號碼且過帳要直接拋入發票管理
                     IF B02_1.F23='1' .AND. B02_1.F22='21'                 &&且應稅且為三聯式發票再加入應付帳款中                  
                          IF B02.F02<=IIF(SEEK(B02.F07,'D01'),D01.F15,'')
                              SELE D19         &&應付帳款明細
                          ELSE
                              SELE D1I       &&應付帳款到下個月或暫存檔
                          ENDIF                          
                          APPEND BLANK
                          REPLACE  F01 WITH CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02_1.F02)
                          REPLACE  F02 WITH B02_1.F01
                          REPLACE  F03 WITH B02_1.F07
                          REPLACE  F04 WITH '稅額'
                          REPLACE  F05 WITH '發票:'+B02_1.F20
                          REPLACE  F06 WITH 1
                          REPLACE  F07 WITH ROUND(TQTY*INT_002/100,INT_068)
                          REPLACE  F08 WITH ROUND(TQTY*INT_002/100,INT_068)
                          REPLACE  F09 WITH IIF(SEEK(B02_1.F07,'D01'),D01.F13,'1')
                          REPLACE  F12 WITH DTCX(DATE())+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
                          REPLACE  F13 WITH INT_011
                          REPLACE  F14 WITH 1
                          REPLACE  F20 WITH B02_1.F20                      
                     ENDIF
                     IF B02_1.F02<=IIF(SEEK(B02_1.F07,'D01'),D01.F15,'')
                         SELE K25         &&發票明細檔
                      ELSE
                        SELE K2E       &&發票明細到下個月或暫存檔
                     ENDIF                          
                     APPEND BLANK
                     REPLACE  F01 WITH B02_1.F22
                     IF B02_1.F02<=IIF(SEEK(B02_1.F07,'D01'),D01.F15,'')
                         REPLACE  F02 WITH B02.F02
                         REPLACE  F19 WITH THISFORM.MTH_LIST.DISPLAYVALUE                       
                     ELSE
                         REPLACE  F02 WITH '01'
                         LON1=PADL(ALLTRIM(STR(VAL(RIGHT(THISFORM.MTH_LIST.DISPLAYVALUE,2))+1)),2,'0')
                         LON2=IIF(LON1>'12',PADL(ALLTRIM(STR(VAL(RIGHT(THISFORM.MTH_LIST.DISPLAYVALUE,4))+1)),4,'0')+'/01',LEFT(THISFORM.MTH_LIST.DISPLAYVALUE,5)+LON1)
                         REPLACE F19 WITH LON2
                     ENDIF                                                                  
                     REPLACE  F03 WITH B02_1.F07
                     REPLACE  F04 WITH IIF(SEEK(B02_1.F07,'D01'),D01.F06,'')
                     REPLACE  F07 WITH B02_1.F20
                     REPLACE  F09 WITH B02_1.F23
                     REPLACE  F10 WITH ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0) &&營業稅額       
                     IF F01='21'
                         REPLACE  F08 WITH TQTY
                         REPLACE  F12 WITH F08+F10
                     ELSE
                         REPLACE  F12 WITH TQTY
                         REPLACE  F08 WITH F12-F10
                     ENDIF   
                     REPLACE  F11 WITH '1'
                     REPLACE  F13 WITH '00'
                     REPLACE  F15 WITH B02_1.F01+IIF(B01.F98='* *','商品','原料')
                     REPLACE  F16 WITH '1'
                     REPLACE  F18 WITH '01'
                     REPLACE F20 WITH ALLTRIM(B02_1.F21)  
                     REPLACE F21 WITH B02.F16      &&幣別
                     REPLACE F22 WITH B02.F18      &&匯率                    
                     REPLACE F23 WITH FTTL
                     REPLACE  F24 WITH ALLTRIM(CHK_NAME)
                       *********以類別分類
                       IF B02_1.F02<=IIF(SEEK(B02_1.F07,'D01'),D01.F15,'')
                           SELE K24         &&發票明細檔
                       ELSE
                           SELE K2D       &&發票明細到下個月或暫存檔
                       ENDIF                         
                       SET ORDER TO 1
                       SEEK B02_1.F22
                       IF FOUND()      
                           REPLACE F04 WITH F04+(ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0)) &&營業稅額 
                           IF B02_1.F22='21'                                          
                               REPLACE F03 WITH F03+TQTY         												&&銷售金額
                               REPLACE F05 WITH F05+(TQTY+(ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0)))	&&發票總額									
                           ELSE
                               REPLACE F05 WITH F05+TQTY				 							               &&發票總額
                               REPLACE F03 WITH F03+(TQTY-(ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0)))    &&銷售金額                 			
                           ENDIF               
                           DO CASE
                                  CASE  B02_1.F23='1'
                          		      REPLACE F06 WITH F06+1    &&應稅張數
                          	  CASE  B02_1.F23='2'
                          	 	      REPLACE F07 WITH F07+1    &&零稅張數
                          	  CASE  B02_1.F23='3'	      
                          	  	      REPLACE F08 WITH F08+1   &&免稅張數
                           ENDCASE		     
                       ELSE
                           APPEND BLANK 
                           REPLACE F01 WITH B02_1.F22
                           REPLACE F04 WITH ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0) &&營業稅額 
                           IF B02_1.F22='21'                                          
                               REPLACE F03 WITH TQTY         		   &&銷售金額
                               REPLACE F05 WITH  F03+F04  &&發票總額									
                           ELSE
                               REPLACE F05 WITH TQTY			   &&發票總額
                               REPLACE F03 WITH  F05-F04   &&銷售金額                 			
                           ENDIF    
                           DO CASE
                                  CASE  B02_1.F23='1'
                          		      REPLACE F06 WITH 1   &&應稅張數
                          	  CASE  B02_1.F23='2'
                          	 	      REPLACE F07 WITH 1    &&零稅張數
                          	  CASE  B02_1.F23='3'	      
                          	  	      REPLACE F08 WITH 1   &&免稅張數
                           ENDCASE
                        ENDIF  
                        ***以對象編號分類
                       SET ORDER TO 2
                       SEEK B02_1.F07
                       IF FOUND()      
                           REPLACE F04 WITH F04+(ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0)) &&營業稅額 
                           IF B02_1.F22='21'                                          
                               REPLACE F03 WITH F03+TQTY         												&&銷售金額
                               REPLACE F05 WITH F05+(TQTY+(ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0)))	&&發票總額									
                           ELSE
                               REPLACE F05 WITH F05+TQTY				 							               &&發票總額
                               REPLACE F03 WITH F03+(TQTY-(ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0)))    &&銷售金額                 			
                           ENDIF               
                           DO CASE
                                  CASE  B02_1.F23='1'
                          		      REPLACE F06 WITH F06+1    &&應稅張數
                          	  CASE  B02_1.F23='2'
                          	 	      REPLACE F07 WITH F07+1    &&零稅張數
                          	  CASE  B02_1.F23='3'	      
                          	  	      REPLACE F08 WITH F08+1   &&免稅張數
                           ENDCASE		     
                       ELSE
                           APPEND BLANK 
                           REPLACE F02 WITH B02_1.F07
                           REPLACE F04 WITH ROUND(TQTY*INT_002/100,INT_068)*IIF(B02_1.F23='1',1,0) &&營業稅額 
                           IF B02_1.F22='21'                                          
                               REPLACE F03 WITH TQTY         		   &&銷售金額
                               REPLACE F05 WITH  F03+F04  &&發票總額									
                           ELSE
                               REPLACE F05 WITH TQTY			   &&發票總額
                               REPLACE F03 WITH  F05-F04   &&銷售金額                 			
                           ENDIF    
                           DO CASE
                                  CASE  B02_1.F23='1'
                          		      REPLACE F06 WITH 1   &&應稅張數
                          	  CASE  B02_1.F23='2'
                          	 	      REPLACE F07 WITH 1    &&零稅張數
                          	  CASE  B02_1.F23='3'	      
                          	  	      REPLACE F08 WITH 1   &&免稅張數
                           ENDCASE
                        ENDIF                         
                         ********                   
                 ENDIF                                                   
                 SELE D01                     &&廠商基本主檔
                 SEEK B02_1.F07
                 IF FOUND()
                    IF F14<CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02_1.F02)
                       REPL F14 WITH CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02_1.F02)
                    ENDIF   
                 ENDIF                    
                 SELE B39                    &&未過帳庫存單據查詢檔
                 SEEK 'B02'+B02_1.F01
                 IF FOUND()
                    DELE
                 ENDIF                                             
                 SELE B02_1
                 REPL F10 WITH '2'
                 REPL F90 WITH DTCX(DATE())+IIF(SEEK(sys_oper,'A01'),A01.F03,'')
               ELSE
                  =MESSAGEBOX('此單據已由別的工作站過帳完成',0+64,'提示訊息視窗')
                  SELE B02_1
                  REPL F10 WITH '2'
               ENDIF           
               THIS.ENABLED=.F.
               THISFORM.ORPGROUP.EDIT_BOTT.ENABLED=.F.
               THISFORM.ORPGROUP.DEL_BOTT.ENABLED=.F.             
               UNLOCK ALL
               IF THISFORM.PAGEFRAME1.ACTIVEPAGE=1
                  THISFORM.GRID1.SETFOCUS
               ELSE
                   THISFORM.GRID2.SETFOCUS
                   THISFORM.ORPGROUP.NEW_BOTT.ENABLED=.F.
               ENDIF
               THISFORM.REFRESH      
            ELSE
              DO file_impact
              unlock all
              return
            ENDIF   
       unlock all
       RELEASE ALL LIKE BK*
    ENDIF   
  ENDPROC
   
  PROC VRT_BOTT.CLICK     &&反過帳程序  
    if messagebox('是否確認資料無誤可以過帳?',4+32+256,'請確認') = 6	 
           IF !EMPTY(B02.F20)
              IF !USED('K51')
                 SELECT 0
                 USE K51
              ELSE
                 SELECT K51
              ENDIF
              SET ORDER TO 1      
              SEEK B02.F20                
              IF FOUND()
                 =MESSAGEBOX('K09已有應付帳款請先去反帳款後再做反過帳',0+48,'提示訊息視窗')
                 RETURN
              ENDIF
              SELECT K51
              USE
          ENDIF          
       SELE RECNO() FROM B02 WHERE B02.F01=B02_1.F01 AND F10='2' INTO ARRAY BK1              
            JJ1=''                
            IF _TALLY>0   
               FOR I= 1 TO ALEN(BK1)
                   JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
                   SELE B02
                   GO BK1(I)
                   IF B02.F13='Y'
                      SELE H01
                      SEEK B02.F03+B02.F01+B02.F09
                      IF FOUND()
                         IF F06-F05<>B02.F04+B02.F15                  
                            =MESSAGEBOX('H02待檢驗資料中尚有未從H06轉入之反過帳資料!,請先至H06反過帳刪除後再做此筆反過帳!',0+48,'提示訊息視窗')
                            THISFORM.PAGEFRAME1.ACTIVEPAGE=1
                            THISFORM.REFRESH                         
                            RETURN          
                         ENDIF                                              
                      ELSE
