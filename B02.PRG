			   ELSE
		                 IF B02_1.F02<=IIF(SEEK(B02_1.F07,'D01'),D01.F15,'')
		                     K24Rec='K24'
		                     SELECT  K24         &&發票明細檔表頭
		                 ELSE
		                     K24Rec='K2D'
		                     SELECT  K2D        &&發票明細到下個月或暫存檔表頭
