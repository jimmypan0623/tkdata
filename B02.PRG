                                            ENDIF
                                       ENDIF	                               
	                          ENDIF                 
	                          SELECT  &K25Rec                      
	                          DELE FOR LEFT(ALLTRIM(F15),10)=ALLTRIM(B02_1.F01)
	                  ENDIF        
                      ENDIF       
                  ENDIF                                                   
               ELSE
                   =MESSAGEBOX('此單據已由別的工作站過帳完成',0+64,'提示訊息視窗')
                  SELE B02_1
                  REPL F10 WITH ' '
