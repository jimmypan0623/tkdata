                                  REPL F03 WITH B02.F03
                                  REPL F04 WITH (B02.F04+B02.F15)*(-1)
                                  REPL F05 WITH CTOD((THISFORM.MTH_LIST.DISPLAYVALUE+'/'+B02.F02))
