  PROC ORPGROUP.DEL_BOTT.CLICK                    &&刪除資料程序
       IF messagebox('是否確定要刪除此筆資料',4+32+256,'請確認') = 6            
          if ALIA()='B02_1'           &&刪除整張訂單
             SELE B02_1             
                PO_NO=B02_1.F01
                SELE RECNO() FROM B02 WHERE B02.F01=PO_NO  AND B02.F10<>'2' INTO ARRAY BK1              
                JJ1=''                
                IF _TALLY>0   
                   FOR I= 1 TO ALEN(BK1)
                       JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
                   ENDFOR    
                   KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')              
                   RLOCK(KK1,'B02')
                   LL1=RLOCK(KK1,'B02')
                ELSE
                   LL1=.T.   
                ENDIF                   
                SELE RECNO() FROM D07 WHERE F01= ANY (SELE F09 FROM B02 WHERE F01=PO_NO) INTO ARRAY BK2
                JJ2=''                
                IF _TALLY>0                   
                    FOR I= 1 TO ALEN(BK2)
                        JJ2=JJ2+ALLTRIM(STR(BK2(I)))+','
                    ENDFOR  
                    KK2=STUFF(JJ2,RAT(',',JJ2,1),1,'')
                    RLOCK(KK2,'D07')
                    LL2=RLOCK(KK2,'D07')
                ELSE
                   LL2=.T.   
                ENDIF   
                SELE RECNO() FROM D04 WHERE F01+F02=ANY (SELE F09+F03 FROM B02 WHERE F01=PO_NO) INTO ARRAY BK3
                JJ3=''                
                IF _TALLY>0   
                   FOR I= 1 TO ALEN(BK3)
                       JJ3=JJ3+ALLTRIM(STR(BK3(I)))+','
                   ENDFOR    
                   KK3=STUFF(JJ3,RAT(',',JJ3,1),1,'')              
                   RLOCK(KK3,'D04')
                   LL3=RLOCK(KK3,'D04')
                ELSE
                   LL3=.T.   
                ENDIF                   
                IF LL1 .AND. LL1 .AND. LL2 .AND. LL3=.T.
                   IF LEN(ALLTRIM(JJ1))>0 
                      SELE B02
                      FOR I= 1 TO ALEN(BK1)
                          GO BK1(I) 
                          SELE D07
                          SET ORDER TO D077                   
                          SEEK B02.F09+B02.F03
                            IF FOUND()
                               QTY=B02.F04
                               DO WHILE F01+F02=B02.F09+B02.F03 
                                  IF F06=0
                                     SKIP
                                  ELSE
                                     IF QTY>F06
                                        QTY=QTY+F06*(-1)
                                        REPL F06 WITH 0
                                        SKIP
                                     ELSE
                                        REPL F06 WITH F06+QTY*(-1)
                                        EXIT
                                     ENDIF  
                                  ENDIF                    
                               ENDDO                                  
                            ENDIF                                 
                            SELE D04
                            SEEK B02.F09+B02.F03
                            IF FOUND()
                                REPL F18 WITH F18+(B02.F04)*(-1)
                            ENDIF    
                            SELE B02
                       ENDFOR                      
                   ELSE
                      =MESSAGEBOX('此張單據已由別的工作站過帳中,無法刪除!',0+64,'提示訊息視窗')
                      SELE B02_1
                      REPL F10 WITH '2'
                      UNLOCK ALL
                      RETURN   
                   ENDIF    
                   DELE FROM B02 WHERE B02.F01=PO_NO
                   SELE B39                     &&刪除未過帳單據紀錄
                   SEEK A02.F03+B02_1.F01                   
                   IF FOUND()
                      DELE
                   ENDIF   
                   SELE B02_1
                   DELE
                   SKIP -1
                   IF BOF()
                      GO TOP
                   ENDIF   
                   UNLOCK ALL
                 IF INT_099=.T. .AND. SUBSTR(PO_NO,3,4)=SUBSTR(DTOS(CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/01')),3,4)
                      HD=HD1+SUBSTR(DTOS(CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/01')),3,4)+PO_NO
                      DO ODR_RJT
                  ENDIF  

                ELSE
                   DO file_impact                  
                ENDIF
             
             THISFORM.GRID1.SETFOCUS
             IF THISFORM.GRID1.ACTIVEROW=0
                THISFORM.CMDGROUP.ENABLED=.F.
                THISFORM.ORPGROUP.EDIT_BOTT.ENABLED=.F.
                THISFORM.ORPGROUP.DEL_BOTT.ENABLED=.F.               
                THISFORM.ORPGROUP.PNT_BOTT.ENABLED=.F.
                THISFORM.ANS_BOTT.ENABLED=.F.
                THISFORM.VRT_BOTT.ENABLED=.F.
             ELSE      
                THISFORM.CMDGROUP.ENABLED=.T.
                THISFORM.ORPGROUP.EDIT_BOTT.ENABLED=IIF(A02.F05='*',.T.,.F.)  .AND. B02_1.F10<>'2' &&判斷有無修改的權限
                THISFORM.ORPGROUP.DEL_BOTT.ENABLED=IIF(A02.F06='*',.T.,.F.)   .AND. B02_1.F10<>'2' &&判斷有無刪除的權限
                THISFORM.ORPGROUP.PNT_BOTT.ENABLED=IIF(A02.F07='*',.T.,.F.)   &&判斷有無列印的權限
                THISFORM.ANS_BOTT.ENABLED=IIF(B02_1.F10='2',.F.,.T.) .AND. IIF(A02.F11='*',.T.,.F.)  .AND. !THISFORM.SHRGROUP.ENABLED      
                THISFORM.VRT_BOTT.ENABLED=IIF(EMPTY(B02_1.F10),.F.,.T.) .AND. IIF(A02.F12='*',.T.,.F.) .AND. !THISFORM.SHRGROUP.ENABLED .AND. !IIF(SEEK(DTOS(CTOD(THISFORM.MTH_LIST.DISPLAYVALUE+'/01')),'A23','A231'),A23.F07,.F.)                         
             ENDIF     
          ELSE        
              SELE B02                                                      &&刪除單筆資料及其進貨計劃        
              SELE RECNO() FROM D07 WHERE F01=B02.F09 AND F02=B02.F03 INTO ARRAY BK1
              JJ1=''                
                IF _TALLY>0                   
                    FOR I= 1 TO ALEN(BK1)
                        JJ1=JJ1+ALLTRIM(STR(BK1(I)))+','
                    ENDFOR  
                    KK1=STUFF(JJ1,RAT(',',JJ1,1),1,'')
                    RLOCK(KK1,'D07')
                    LL1=RLOCK(KK1,'D07')
                ELSE
                   LL1=.T.   
                ENDIF 
                SELE D04
                SEEK B02.F09+B02.F03
                IF FOUND()
                   RLOCK()
                   LL2=RLOCK()
                ELSE
                   LL2=.T.   
                ENDIF  
                 IF RLOCK('B02') .AND. LL1=.T. .AND. LL2=.T.
                    IF LEN(ALLTRIM(JJ1))>0
                       SELE D07
                       SET ORDER TO D077
                       SEEK B02.F09+B02.F03
                       IF FOUND()
                          QTY=B02.F04
                          DO WHILE F01+F02=B02.F09+B02.F03 
                             IF F06=0
                                SKIP
                             ELSE
                                IF QTY>F06
                                   QTY=QTY+F06*(-1)
                                   REPL F06 WITH 0
                                   SKIP
                                ELSE
                                   REPL F06 WITH F06+QTY*(-1)
                                   EXIT
                                ENDIF  
                             ENDIF                    
                           ENDDO                                  
                        ENDIF                                 
                     ENDIF                        
                     SELE D04
                     SEEK B02.F09+B02.F03
                     IF FOUND()
                        REPL F18 WITH F18+(B02.F04)*(-1)
                     ENDIF                       
                     SELE B02
                     DELE
                     SKIP -1
                     IF BOF()
                        GO TOP
                     ENDIF   
                  ELSE
                    DO file_impact                  
