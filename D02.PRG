 PROCEDURE CMND1.CLICK 
   IF AREA1='D01'  
      LK=D01.F01   
      IF O_TP='1'
         SELECT D02.F01,D02.F02,D02.F03,D02.F05,D02.F06,D02.F07,D02.F08,D02.F09,D02.F10,D02.F13,B01.F02,D01.F03 FROM D02,B01,D01;
             WHERE D02.F01 = LK AND B01.F01 = D02.F03 AND D01.F01 = D02.F01 AND IIF(THISFORM.DATA_OPTION.VALUE = 1,.T.,D02.F02 >= THISFORM.TEXT1.VALUE AND D02.F02 <= THISFORM.TEXT2.VALUE) ORDER BY D02.F03,D02.F02 NOWAIT            
         IF _tally >0            
               REPORT FORM ALLTRIM(INT_116)+'D02A' TO PRINT PROMPT PREVIEW 
            FLG = '1'
         ELSE
            FLG = '0'   
         ENDIF               
      ELSE  
         SELECT D02.F01 廠商編號,D01.F04 廠商簡稱,D02.F03 料品編號,B01.F02 品名規格,D02.F02 詢價日期,D02.F06 幣別,D02.F07 單價,D02.F08 最小訂購量,D02.F09 交貨天數,D02.F13 包裝基量,D02.F10 交易條件 FROM D02,B01,D01 ORDER BY D02.F03,D02.F02;
                WHERE D02.F01 = LK AND B01.F01 = D02.F03 AND D01.F01 = D02.F01 AND IIF(THISFORM.DATA_OPTION.VALUE = 1,.T.,D02.F02 >= THISFORM.TEXT1.VALUE AND D02.F02 <= THISFORM.TEXT2.VALUE)  NOWAIT
	        IF _TALLY>0               
               FILE_NAME=LK+ALLTRIM(IIF(SEEK(LK,'D01'),D01.F04,''))+'詢價紀錄'   	           	        
   	           GCDELIMFILE=PUTFILE('儲存路徑',FILE_NAME,'XLS')
	           IF EMPTY(GCDELIMFILE)  && ESC PRESSED	          
                  RETURN
               ELSE
                 ON ERROR DO FILE_IMPACT
                 COPY TO (GCDELIMFILE) TYPE XL5   
		         =MESSAGEBOX('儲存成功',0+64,'提示訊息視窗')              
               ENDIF    
               FLG='1'
            ELSE
               FLG='0'   
            ENDIF   
       ENDIF     
   ELSE
      LK=B01.F01   
      IF O_TP='1'
         SELECT D02.F01,D02.F02,D02.F03,D02.F05,D02.F06,D02.F07,D02.F08,D02.F09,D02.F10,D02.F13,B01.F02,D01.F03 FROM D02,B01,D01;     
         WHERE D02.F03 = LK AND B01.F01 = D02.F03 AND D01.F01 = D02.F01 AND IIF(THISFORM.DATA_OPTION.VALUE = 1,.T.,D02.F02 >= THISFORM.TEXT1.VALUE AND D02.F02 <= THISFORM.TEXT2.VALUE) ORDER BY D02.F01,D02.F02 NOWAIT
         IF _tally >0
            REPORT FORM ALLTRIM(INT_116)+'D02B' TO PRINT PROMPT PREVIEW
            FLG = '1'
         ELSE
            FLG = '0'
         ENDIF  
      ELSE
         SELECT D02.F01 廠商編號,D01.F04 廠商簡稱,D02.F02 詢價日期,D02.F03 料品編號,B01.F02 品名規格,D02.F06 幣別,D02.F07 單價,D02.F08 最小採購量,D02.F09 交貨天數,D02.F10 交易條件,D02.F13 包裝基量 FROM D02,B01,D01;     
             WHERE D02.F03 = LK AND B01.F01 = D02.F03 AND D01.F01 = D02.F01 AND IIF(THISFORM.DATA_OPTION.VALUE = 1,.T.,D02.F02 >= THISFORM.TEXT1.VALUE AND D02.F02 <= THISFORM.TEXT2.VALUE) ORDER BY D02.F01,D02.F02 NOWAIT      
	        IF _TALLY>0               
               FILE_NAME=ALLTRIM(LK)+'詢價紀錄'   	           	        
   	           GCDELIMFILE=PUTFILE('儲存路徑',FILE_NAME,'XLS')
	           IF EMPTY(GCDELIMFILE)  && ESC PRESSED	          
                  RETURN
               ELSE
                 ON ERROR DO FILE_IMPACT
                 COPY TO (GCDELIMFILE) TYPE XL5   
		         =MESSAGEBOX('儲存成功',0+64,'提示訊息視窗')              
               ENDIF    
               FLG='1'
            ELSE
               FLG='0'   
            ENDIF   
      ENDIF           
   ENDIF  
   O_TP=''
   THISFORM.RELEASE 
 ENDPROC      
 PROCEDURE CMND3.CLICK
   O_TP='' 
   FLG='1'   
   THISFORM.RELEASE
 ENDPROC        
